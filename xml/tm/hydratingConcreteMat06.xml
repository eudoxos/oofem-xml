<oofem version="1">
  <Output>hydratingConcreteMat06.out</Output>
  <Description>Test of hydrating concrete material, one linear triangluar element, transient transport solver, isothermal conditions, Rahimi-Agham's model</Description>
  <Analysis type="TransientTransport" nsteps="30" deltat="7200.0" alpha="0.5"/>
  <!-- vtkxml tstep_step 1 domain_all primvars 1 6 vars 4 37 39 56 96 stype 1 -->
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="1.0 0.0 0.0"/>
      <Node coords="2.2 0.0 0.0"/>
      <Node coords="1.0 1.0 0.0"/>
    </Nodes>
    <Elements>
      <tr1ht nodes="1 2 3" crosssect="1"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS thickness="0.15" mat="1"/>
    </CrossSections>
    <Materials>
      <!-- Standard concrete k(conductivity)=1.7 W/m/K   c(capacity)=870 J/kg/K -->
      <HydratingConcreteMat d="2458.0" k="1.7" c="870.0" hydrationmodeltype="3" qpot="509.0" masscement="400.0" w_c="0.5" a_c="5.0" castingtime="0.0" maturityt0="20.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="20.0" set="1"/>
    </BoundaryConditions>
    <InitialConditions>
      <InitialCondition conditions="u 20.0" dofs="10" set="1"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.001">
      <TIME/>
      <!-- ELEMENT tStep 30 number 1 gp 1 keyword 39 component 1 value 5.20334463e-01 -->
      <!-- ELEMENT tStep 6 number 2 gp 1 keyword 39 component 1 value 1.07180394e-01 -->
      <!-- ELEMENT tStep 30 number 2 gp 1 keyword 39 component 1 value 6.48537723e-01 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="39" component="1" value="0.05"/>
    </errorcheck>
  </ExportModules>
</oofem>
