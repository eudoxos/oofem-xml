    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="11" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="11" values="1.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
