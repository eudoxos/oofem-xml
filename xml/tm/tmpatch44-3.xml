      <ELEMENT tStep="1" number="1" gp="1" keyword="56" component="1" value="-208.333009"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="56" component="1" value="-207.963215"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="56" component="1" value="-51.9147254"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="56" component="1" value="-47.2466968"/>
