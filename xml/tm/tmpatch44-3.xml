      <deadweight loadtimefunction="1" components="50.0" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="1 4"/>
    </Sets>
  </Domain>
  <ExportModules>
