<oofem version="1">
  <Output>tmpatch15.out</Output>
  <Description>Patch test of Quad1_ht elements, nonstationary</Description>
  <Analysis type="TransientTransport" nsteps="30" deltat="600000.0" rtolf="1e-06" alpha="0.5" miniter="1"/>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
      <Node coords="2.0 1.0 0.0"/>
      <Node id="5" coords="0.0 8.0 0.0"/>
      <Node coords="2.0 8.0 0.0"/>
    </Nodes>
    <Elements>
      <quad1ht nodes="1 3 4 2"/>
      <!-- quad1ht 2 nodes 4 3 5 6 4 boundaryloads 2 2 2 -->
      <quad1ht nodes="3 5 6 4"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" set="1" thickness="0.15"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="2400.0" k="3.0" c="1000.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="0.0" set="2"/>
      <!-- Newton bc -->
      <!-- constantedgeload 2 loadTimeFunction 1 components 1 15.0 properties 1 a 0.8 loadtype 3 -->
      <constantedgeload loadtimefunction="1" components="15.0" properties="a 0.8" loadtype="3" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2"/>
      <Set elementedges="2 2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="20" number="3" dof="10" unknown="d" value="0.815996152" tolerance="1e-05"/>
      <NODE tStep="20" number="5" dof="10" unknown="d" value="8.96042091" tolerance="1e-05"/>
      <NODE tStep="30" number="3" dof="10" unknown="d" value="1.10196762" tolerance="1e-05"/>
      <NODE tStep="30" number="4" dof="10" unknown="d" value="1.10196762" tolerance="1e-05"/>
      <NODE tStep="30" number="5" dof="10" unknown="d" value="9.73800585" tolerance="1e-05"/>
      <NODE tStep="30" number="6" dof="10" unknown="d" value="9.73800585" tolerance="1e-05"/>
    </errorcheck>
  </ExportModules>
</oofem>
