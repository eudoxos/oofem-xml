<oofem version="1">
  <Output>tmpatch46.out</Output>
  <Description>Patch test of IsoHeatMat with temperature-dependent material parameters</Description>
  <!-- TransientTransport nsteps 50 deltat 100.0 rtolf 1.e-6 alpha 0.6 lstype 1 lstol 1e-6 lsiter 200 lsprecond 1 renumber 1 exportfields 1 5 nmodules 1 -->
  <Analysis type="TransientTransport" nsteps="50" deltat="100.0" rtolf="1e-06" alpha="0.6" lstype="0" renumber="1" exportfields="5"/>
  <Domain domain="heattransfer">
    <OutputManager/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 0.0 1.0"/>
      <Node coords="1.0 0.0 1.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node id="5" coords="0.0 1.0 0.0"/>
      <Node coords="0.0 1.0 1.0"/>
      <Node coords="1.0 1.0 1.0"/>
      <Node coords="1.0 1.0 0.0"/>
    </Nodes>
    <Elements>
      <brick1ht nodes="1 2 3 4 5 6 7 8"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="800.0" k="@2" c="@3" td="@4"/>
    </Materials>
    <BoundaryConditions>
      <constantsurfaceload loadtimefunction="1" components="500.0" properties="a 10.0" loadtype="3" set="2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <!-- wood thermal conductivity -->
      <Piecewiselinfunction t="-100.0 20.0 200.0 350.0 500.0 800.0 1200.0" f_t_="0.12 0.12 0.15 0.07 0.09 0.35 1.5" paramtype="1"/>
      <!-- wood thermal capacity -->
      <Piecewiselinfunction t="-100.0 20.0 150.0 200.0 250.0 300.0 800.0 1200.0" f_t_="1600.0 1600.0 2000.0 2000.0 1600.0 850.0 1600.0 1600.0" paramtype="1"/>
      <!-- wood density -->
      <Piecewiselinfunction t="-100.0 200.0 400.0 1200.0" f_t_="800.0 800.0 350.0 0.0" paramtype="1"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-1"/>
      <Set elementboundaries="1 6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="25" number="1" dof="10" unknown="d" value="37.4825883"/>
      <NODE tStep="50" number="1" dof="10" unknown="d" value="71.1157261"/>
    </errorcheck>
  </ExportModules>
</oofem>
