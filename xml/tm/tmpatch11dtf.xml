<oofem version="1">
  <Output>tmpatch11dtf.out</Output>
  <Description>Patch test of Quad1_ht elements</Description>
  <Analysis type="TransientTransport" nsteps="20" dtfunction="2" alpha="0.5"/>
  <!-- vtkxml tstep_all domain_all primvars 1 6 vars 2 37 56 stype 1 -->
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="2.0 4.0 0.0"/>
      <Node id="5" coords="4.0 0.0 0.0"/>
      <Node coords="4.0 4.0 0.0"/>
    </Nodes>
    <Elements>
      <quad1ht nodes="1 3 4 2"/>
      <quad1ht nodes="3 5 6 4"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" set="1" thickness="0.15"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="2400.0" k="1." c="1000.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="15.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions>
      <InitialCondition conditions="u 15." dofs="10" set="3"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <UsrDefLTF f_t_="2^(t-2)"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2"/>
      <Set nodes="5 6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="20" number="3" dof="10" unknown="d" value="1.1337489"/>
      <ELEMENT tStep="20" number="1" gp="1" keyword="56" component="1" value="-0.430925146"/>
    </errorcheck>
  </ExportModules>
</oofem>
