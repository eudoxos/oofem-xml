<oofem version="1">
  <Output>tmquad12.out</Output>
  <Description>Test of Quad1_ht elements with the change of problem size</Description>
  <Analysis type="transienttransport" nsteps="20" deltat="600.0" alpha="0.5" miniter="1"/>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="2.0 4.0 0.0"/>
      <Node id="5" coords="4.0 0.0 0.0"/>
      <Node coords="4.0 4.0 0.0"/>
    </Nodes>
    <Elements>
      <quad1ht nodes="1 3 4 2"/>
      <quad1ht nodes="3 5 6 4"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" thickness="0.15" set="1"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="2400.0" k="1.0" c="2.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="15.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="25.0" isimposedtimefunction="2" set="3"/>
    </BoundaryConditions>
    <InitialConditions>
      <InitialCondition conditions="u 3.0" dofs="10" set="4"/>
      <InitialCondition conditions="u 15.0" dofs="10" set="2"/>
      <InitialCondition conditions="u 25.0" dofs="10" set="3"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <UsrDefLTF f_t_="1-h(1802)"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="5 6"/>
      <Set nodes="1 2"/>
      <Set nodes="3 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="1" number="3" dof="10" unknown="d" value="4.52238806"/>
      <NODE tStep="4" number="1" dof="10" unknown="d" value="22.6895994"/>
      <NODE tStep="4" number="3" dof="10" unknown="d" value="8.81930722"/>
      <NODE tStep="20" number="1" dof="10" unknown="d" value="14.7733464"/>
      <NODE tStep="20" number="3" dof="10" unknown="d" value="14.6229774"/>
    </errorcheck>
  </ExportModules>
</oofem>
