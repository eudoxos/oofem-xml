<oofem version="1">
  <Output>tmpatch11.out</Output>
  <Description>Patch test of Quad1_ht elements</Description>
  <!--StationaryProblem nsteps 1-->
  <Analysis type="TransientTransport" nsteps="200" deltat="600000.0" alpha="0.5" rtolf="1e-09" miniter="1" maxiter="300" lumped=""/>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="2.0 4.0 0.0"/>
      <Node id="5" coords="4.0 0.0 0.0"/>
      <Node coords="4.0 4.0 0.0"/>
    </Nodes>
    <Elements>
      <quad1ht nodes="1 3 4 2"/>
      <quad1ht nodes="3 5 6 4"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" set="1" thickness="0.15"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="2400.0" k="1.0" c="1000.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="15.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2"/>
      <Set nodes="5 6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <!--NODE tStep 9 number 3 dof 10 unknown d value 6.11951769e+00-->
      <NODE tStep="9" number="3" dof="10" unknown="d" value="4.90659093"/>
      <NODE tStep="200" number="3" dof="10" unknown="d" value="7.5"/>
    </errorcheck>
  </ExportModules>
</oofem>
