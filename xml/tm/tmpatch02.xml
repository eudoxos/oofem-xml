<oofem version="1">
  <Output>tmpatch02.out</Output>
  <Description>Patch test of Quad1_ht elements</Description>
  <Analysis type="StationaryProblem" nsteps="1"/>
  <!-- vtkxml tstep_all domain_all primvars 1 6 -->
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
      <Node coords="0.0 8.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node id="5" coords="2.0 1.0 0.0"/>
      <Node coords="2.0 8.0 0.0"/>
    </Nodes>
    <Elements>
      <quad1ht nodes="1 2 5 4"/>
      <quad1ht nodes="2 3 6 5"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" set="1" thickness="0.15"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="0.0" k="1.0" c="1.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="15.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 4"/>
      <Set nodes="3 6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="1" number="2" dof="10" unknown="d" value="1.875"/>
      <NODE tStep="1" number="5" dof="10" unknown="d" value="1.875"/>
    </errorcheck>
  </ExportModules>
</oofem>
