<oofem version="1">
  <Output>tmpatch39.out</Output>
  <Description>Patch test of Brick_ht elements, heat transfer coefficient depending on temperature</Description>
  <!--StationaryProblem nsteps 1 nmodules 1-->
  <Analysis type="TransientTransport" nsteps="5" deltat="50000.0" alpha="0.5" lumped="" rtolf="1e-06" miniter="1"/>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="2.0 2.0 0.0"/>
      <Node coords="0.0 2.0 0.0"/>
      <Node id="5" coords="0.0 0.0 1.0"/>
      <Node coords="2.0 0.0 1.0"/>
      <Node coords="2.0 2.0 1.0"/>
      <Node coords="0.0 2.0 1.0"/>
      <Node coords="0.0 0.0 8.0"/>
      <Node id="10" coords="2.0 0.0 8.0"/>
      <Node coords="2.0 2.0 8.0"/>
      <Node coords="0.0 2.0 8.0"/>
    </Nodes>
    <Elements>
      <brick1ht nodes="1 2 3 4 5 6 7 8"/>
      <brick1ht nodes="5 6 7 8 9 10 11 12"/>
    </Elements>
    <CrossSections>
      <SimpleTransportCS mat="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoHeat d="2400.0" k="3.0" c="1000.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="10" values="5.0" set="2"/>
      <constantsurfaceload loadtimefunction="1" components="15.0" properties="a 1.0" propertymultexpr="$(273 + u)*0.01$" loadtype="3" set="3"/>
    </BoundaryConditions>
    <InitialConditions>
      <InitialCondition conditions="u 5.0" dofs="10" set="1"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2 3 4"/>
      <Set elementboundaries="2 2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="5" number="5" dof="10" unknown="d" value="5.00434159"/>
      <NODE tStep="5" number="6" dof="10" unknown="d" value="5.00434159"/>
      <NODE tStep="5" number="10" dof="10" unknown="d" value="5.79023991"/>
      <NODE tStep="5" number="12" dof="10" unknown="d" value="5.79023991"/>
    </errorcheck>
  </ExportModules>
</oofem>
