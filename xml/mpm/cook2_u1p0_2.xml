<oofem version="1">
  <Output>cook2_u1p0_2.out</Output>
  <Description>Demo of symbolic mpm problem; Cook membrane benchmark</Description>
  <Analysis type="mpmsymbolicstationaryproblem" nsteps="1" lhsterms="1 2 3 4" rhsterms="5" profileopt="1" maxiter="10">
    <MPMVariables>
      <!-- displacement -->
      <Variable name="u" interpolation="feilin" type="vector" quantity="Displacement" size="2" dofs="1 2"/>
      <!-- test function -->
      <Variable name="w" interpolation="feilin" type="vector" quantity="Displacement" size="2" dofs="1 2"/>
      <!-- pressure -->
      <Variable name="p" interpolation="feiconst" type="scalar" quantity="Pressure" size="1" dofs="11"/>
      <!-- test function -->
      <Variable name="dp" interpolation="feiconst" type="scalar" quantity="Pressure" size="1" dofs="11"/>
    </MPMVariables>
    <MPMTerms>
      <BTSigmaTerm variable="u" testvariable="w" mmode="PlaneStrain" lhsmatmode="29" rhsmatmode="30"/>
      <BTamNTerm variable="p" testvariable="w" mmode="PlaneStrain" atype="28"/>
      <NTamTBTerm variable="u" testvariable="dp" mmode="PlaneStrain" atype="28" uvmt="1"/>
      <NTcN variable="p" testvariable="dp" mmode="PlaneStrain" ctype="27" uvmt="1"/>
      <NTfTerm id="5" variable="u" testvariable="w" mmode="PlaneStress" flux="0.0 6.25"/>
    </MPMTerms>
    <MPMIntegrals>
      <!-- NTfTerm 5 variable "u" testvariable "w" mmode 6 flux 2 0. 31.25 -->
      <Integral domain="1" set="1" term="1"/>
      <Integral domain="1" set="1" term="2" factor="-1.0"/>
      <Integral domain="1" set="1" term="3" factor="-1.0"/>
      <Integral domain="1" set="1" term="4" factor="-1.0"/>
      <Integral id="5" domain="1" set="2" term="5"/>
    </MPMIntegrals>
  </Analysis>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="24.0 22.0 0.0"/>
      <Node coords="48.0 44.0 0.0"/>
      <Node coords="0.0 22.0 0.0"/>
      <Node id="5" coords="24.0 37.0 0.0"/>
      <Node coords="48.0 52.0 0.0"/>
      <Node coords="0.0 44.0 0.0"/>
      <Node coords="24.0 52.0 0.0"/>
      <Node coords="48.0 60.0 0.0"/>
    </Nodes>
    <Elements>
      <q1 nodes="1 2 5 4" mat="1" crosssect="1"/>
      <q1 nodes="2 3 6 5" mat="1" crosssect="1"/>
      <q1 nodes="4 5 8 7" mat="1" crosssect="1"/>
      <q1 nodes="5 6 9 8" mat="1" crosssect="1"/>
      <l1 id="5" nodes="3 6" mat="1" crosssect="1"/>
      <l1 nodes="6 9" mat="1" crosssect="1"/>
    </Elements>
    <CrossSections>
      <simplecs thick="5.0"/>
    </CrossSections>
    <Materials>
      <isole d="1.0" e="250.0" n="0.49999" talpha="1.0"/>
    </Materials>
    <BoundaryConditions>
      <!-- clamped-displacement -->
      <boundarycondition loadtimefunction="1" set="3" values="0.0 0.0" dofs="1 2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <constantfunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="4" t="-10.0 0.0 1.0 5.0" f_t_="0.0 0.0 1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set elementranges="5-6"/>
      <Set elementedges="1 4 3 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <!-- check nodes -->
      <NODE tStep="1" number="9" dof="1" unknown="d" value="-3.04792369" tolerance="1e-06"/>
      <NODE tStep="1" number="9" dof="2" unknown="d" value="4.75052676" tolerance="1e-06"/>
    </errorcheck>
  </ExportModules>
</oofem>
