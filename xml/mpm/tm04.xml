<oofem version="1">
  <Output>tm04.out</Output>
  <Description>Test of mpm tm mpm formulation, single element, boundary flux, transient, nonzero initial condition</Description>
  <!--final time 500, initial time step 1-->
  <Analysis type="mpmproblem" nsteps="10" deltat="1.0" alpha="1.0" manrmsteps="0" keeptangent="" ptype="tm" smtype="1"/>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0" ic="1"/>
      <Node coords="3.0 0.0 0.0" ic="1"/>
      <Node coords="3.0 2.0 0.0" ic="1"/>
      <Node coords="0.0 2.0 0.0" ic="1"/>
      <Node id="5" coords="0.0 0.0 1.0" ic="1"/>
      <Node coords="3.0 0.0 1.0" ic="1"/>
      <Node coords="3.0 2.0 1.0" ic="1"/>
      <Node coords="0.0 2.0 1.0" ic="1"/>
    </Nodes>
    <Elements>
      <tmbrick11 nodes="1 2 3 4 5 6 7 8"/>
    </Elements>
    <CrossSections>
      <dummycs mat="1" set="1"/>
    </CrossSections>
    <Materials>
      <!--thermo-mechanical material model-->
      <tmm d="1.0" e="5.0" nu="0.3" lambda="0.2" c="0.5" alpha="1.0"/>
    </Materials>
    <BoundaryConditions>
      <!--x-displacement-->
      <boundarycondition loadtimefunction="1" set="2" values="0.0" dofs="1"/>
      <!--y-displacement-->
      <boundarycondition loadtimefunction="1" set="3" values="0.0" dofs="2"/>
      <!--z-displacement-->
      <boundarycondition loadtimefunction="1" set="4" values="0.0" dofs="3"/>
      <!--temperature prescribed (bleeding=pressure set to zero?)-->
      <boundarycondition loadtimefunction="1" set="5" values="2.0" dofs="10"/>
      <!--prescribed flux (q=1/6.)-->
      <constantsurfaceload id="5" loadtimefunction="2" loadtype="2" dofs="10" cstype="0" components="-0.16666666" set="6"/>
    </BoundaryConditions>
    <InitialConditions>
      <initialcondition set="7" conditions="u 2.0" dofs="10"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <constantfunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="4" t="-10.0 0.0 1.0 100.0" f_t_="0.0 0.0 1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
      <!--x-bc-->
      <Set nodes="1 4 5 8"/>
      <!--y-bc-->
      <Set nodes="1 2 5 6"/>
      <!--z-bc-->
      <Set noderanges="1-4"/>
      <!--top surface (load + )-->
      <Set id="5" noderanges="1-4"/>
      <!--flux-->
      <Set elementboundaries="1 2"/>
      <!--initial temperature-->
      <Set noderanges="1-8"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="10" number="7" dof="3" unknown="d" value="2.25876617" tolerance="1e-05"/>
      <NODE tStep="10" number="7" dof="10" unknown="d" value="2.833333334" tolerance="0.001"/>
    </errorcheck>
  </ExportModules>
</oofem>
