<oofem version="1">
  <Output>tm11.out</Output>
  <Description>patch test of TMTetra11 elements</Description>
  <Analysis type="mpmproblem" nsteps="10" deltat="1.0" alpha="1.0" manrmsteps="0" keeptangent="" ptype="tm" smtype="1"/>
  <Domain domain="3d">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="2.0 3.0 0.0"/>
      <Node coords="0.0 3.0 0.0"/>
      <Node id="5" coords="0.0 0.0 1.0"/>
      <Node coords="2.0 0.0 1.0"/>
      <Node coords="2.0 3.0 1.0"/>
      <Node coords="0.0 3.0 1.0"/>
    </Nodes>
    <Elements>
      <TMTetra11 nodes="1 2 4 5"/>
      <TMTetra11 nodes="5 6 2 4"/>
      <TMTetra11 nodes="5 8 6 4"/>
      <TMTetra11 nodes="3 4 2 7"/>
      <TMTetra11 id="5" nodes="7 8 4 2"/>
      <TMTetra11 nodes="8 7 6 2"/>
    </Elements>
    <CrossSections>
      <dummycs mat="1" set="1"/>
    </CrossSections>
    <Materials>
      <!--thermo-mechanical material model-->
      <tmm d="1.0" e="5.0" nu="0.3" lambda="0.2" c="0.5" alpha="1.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2 3" values="0.0 0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="6"/>
      <!--temperature prescribed (bleeding=pressure set to zero?)-->
      <boundarycondition loadtimefunction="1" set="5" values="2.0" dofs="10"/>
      <!--prescribed flux (q=1/6.)-->
      <!--boundarycondition 4 loadtimefunction 2 set 6 values 1 2.0 dofs 1 10-->
      <!--nodalload 4 loadtimefunction 2 dofs 1 10 Components 1 0.1666666667 set 6-->
      <!--nodalload 4 loadtimefunction 2 dofs 1 3 Components 1 0.0 set 6-->
      <constantsurfaceload loadtimefunction="2" loadtype="3" dofs="10" cstype="0" components="4.0" properties="a 1.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions>
      <initialcondition set="5" conditions="u 2.0" dofs="10"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <constantfunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="4" t="-10.0 0.0 1.0 100.0" f_t_="0.0 0.0 1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-6"/>
      <Set nodes="1 2 3 4"/>
      <Set nodes="1 2 3 4"/>
      <Set elementboundaries="3 1 6 1"/>
      <Set id="5" noderanges="1-4"/>
      <Set/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-05">
      <!--Note: this is the exact solution-->
      <NODE tStep="10" number="7" dof="3" unknown="d" value="4.45590439" tolerance="1e-05"/>
      <NODE tStep="10" number="7" dof="10" unknown="d" value="3.66666666667" tolerance="1e-05"/>
    </errorcheck>
  </ExportModules>
</oofem>
