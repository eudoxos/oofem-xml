<oofem version="1">
  <Output>tm02.out</Output>
  <Description>Test of mpm tm mpm formulation, single element, transient, prescribed nodal flux</Description>
  <!-- final time 500, initial time step 1 -->
  <Analysis type="mpmproblem" nsteps="10" deltat="1.0" alpha="1.0" manrmsteps="0" keeptangent="" ptype="tm" smtype="1"/>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="3.0 0.0 0.0"/>
      <Node coords="3.0 2.0 0.0"/>
      <Node coords="0.0 2.0 0.0"/>
      <Node id="5" coords="0.0 0.0 1.0"/>
      <Node coords="3.0 0.0 1.0"/>
      <Node coords="3.0 2.0 1.0"/>
      <Node coords="0.0 2.0 1.0"/>
    </Nodes>
    <Elements>
      <tmbrick11 nodes="1 2 3 4 5 6 7 8"/>
    </Elements>
    <CrossSections>
      <dummycs mat="1" set="1"/>
    </CrossSections>
    <Materials>
      <!-- thermo-mechanical material model -->
      <tmm d="1.0" e="5.0" nu="0.3" lambda="0.2" c="0.05" alpha="1.0"/>
    </Materials>
    <BoundaryConditions>
      <!-- x-displacement -->
      <boundarycondition loadtimefunction="1" set="2" values="0.0" dofs="1"/>
      <!-- y-displacement -->
      <boundarycondition loadtimefunction="1" set="3" values="0.0" dofs="2"/>
      <!-- z-displacement -->
      <boundarycondition loadtimefunction="1" set="4" values="0.0" dofs="3"/>
      <!-- temperature prescribed (bleeding=pressure set to zero?) -->
      <boundarycondition loadtimefunction="1" set="5" values="0.0" dofs="10"/>
      <!-- temperature  dicrete nodal flux (q=4*0.25/A, T(1) = 5/6 = 0.8333333333333334) -->
      <nodalload id="5" loadtimefunction="2" dofs="10" components="0.25" set="6"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <constantfunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="4" t="-10.0 0.0 1.0 10.0" f_t_="0.0 0.0 1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
      <!-- x-bc -->
      <Set nodes="1 4"/>
      <!-- y-bc -->
      <Set nodes="1 2"/>
      <!-- z-bc -->
      <Set noderanges="1-4"/>
      <!-- top surface (load + ) -->
      <Set id="5" noderanges="1-4"/>
      <!-- flux -->
      <Set nodes="5 6 7 8"/>
      <!-- all nodes -->
      <Set noderanges="1-8"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="10" number="7" dof="3" unknown="d" value="0.416666667" tolerance="1e-05"/>
      <NODE tStep="10" number="7" dof="10" unknown="d" value="0.833333334" tolerance="1e-05"/>
      <NODE tStep="10" number="8" dof="3" unknown="d" value="0.416666667" tolerance="1e-05"/>
      <NODE tStep="10" number="8" dof="10" unknown="d" value="0.833333334" tolerance="1e-05"/>
    </errorcheck>
  </ExportModules>
</oofem>
