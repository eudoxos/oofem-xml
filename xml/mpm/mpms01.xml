<oofem version="1">
  <Output>mpms01.out</Output>
  <Description>Demo of symbolic mpm problem</Description>
  <Analysis type="test" nsteps="1" lhsterms="1" rhsterms="2">
    <MPMVariables>
      <!--displacement-->
      <Variable name="u" interpolation="feilin" type="1" quantity="0" size="2" dofs="1 2"/>
      <!--test function-->
      <Variable name="w" interpolation="feilin" type="1" quantity="0" size="2" dofs="1 2"/>
    </MPMVariables>
    <MPMTerms>
      <BTSigmaTerm variable="u" testvariable="w" mmode="6"/>
      <NTfTerm variable="u" testvariable="w" mmode="6" flux="1.0 0.0"/>
    </MPMTerms>
    <MPMIntegrals>
      <Integral domain="1" set="1" term="1"/>
      <Integral domain="1" set="2" term="2"/>
    </MPMIntegrals>
  </Analysis>
  <Domain domain="heattransfer">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node coords="1.0 1.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
    </Nodes>
    <Elements>
      <q1 nodes="1 2 3 4" mat="1" crosssect="1"/>
      <l1 nodes="2 3" mat="1" crosssect="1"/>
    </Elements>
    <CrossSections>
      <simplecs thick="1.0"/>
    </CrossSections>
    <Materials>
      <isole d="1.0" e="1.0" n="0.3" talpha="1.0"/>
    </Materials>
    <BoundaryConditions>
      <!--x-displacement-->
      <boundarycondition loadtimefunction="1" set="3" values="0.0" dofs="1"/>
      <!--y-displacement-->
      <boundarycondition loadtimefunction="1" set="4" values="0.0" dofs="2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <constantfunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="4" t="-10.0 0.0 1.0 5.0" f_t_="0.0 0.0 1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
      <Set elements="2"/>
      <!--x-bc-->
      <Set nodes="1 4"/>
      <!--y-bc-->
      <Set nodes="1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <!--check nodes-->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="1.0"/>
      <NODE tStep="1" number="2" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="1.0"/>
      <NODE tStep="1" number="3" dof="2" unknown="d" value="-0.3"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="-0.3"/>
    </errorcheck>
  </ExportModules>
</oofem>
