<oofem version="1">
  <Output>frcfcm_shear.out</Output>
  <Description>fixed crack model for fiber reinforced concrete - separate tests on a single finite element</Description>
  <Analysis type="NonLinearStatic" nsteps="31" deltat="1.0" rtolv="1e-06" maxiter="10000" controllmode="1" stiffmode="0" renumber="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <!-- 1: frcfcm_shear_CAF_beta_multiple -->
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.1 0.0 0.0"/>
      <Node coords="0.0 0.1 0.0"/>
      <Node coords="0.1 0.1 0.0"/>
      <!-- 2: frcfcm_shear_SRF_beta_multiple -->
      <Node id="5" coords="0.0 0.0 0.0"/>
      <Node coords="0.1 0.0 0.0"/>
      <Node coords="0.0 0.1 0.0"/>
      <Node coords="0.1 0.1 0.0"/>
      <!-- 3: frcfcm_shear_SRF_damage_exp -->
      <Node coords="0.0 0.0 0.0"/>
      <Node id="10" coords="0.1 0.0 0.0"/>
      <Node coords="0.0 0.1 0.0"/>
      <Node coords="0.1 0.1 0.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2D nodes="1 2 4 3"/>
      <PlaneStress2D nodes="5 6 8 7"/>
      <PlaneStress2D nodes="9 10 12 11"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" width="1.0" material="1" set="7"/>
      <SimpleCS thick="1.0" width="1.0" material="2" set="8"/>
      <SimpleCS thick="1.0" width="1.0" material="3" set="9"/>
    </CrossSections>
    <Materials>
      <frcfcm d="0.0" talpha="0.0" e="20000.0" n="0.2" gf="0.0001" ft="2.0" softtype="2" sheartype="1" multiplecrackshear="" beta="0.01" vf="0.02" df="4e-05" ef="20000.0" gfib="1000.0" tau_0="1.0" f="0.7" kfib="0.9" fsstype="0" fdamtype="0" fibertype="0" fibreorientationvector="10.0 1.0 0.0"/>
      <frcfcm d="0.0" talpha="0.0" e="20000.0" n="0.2" gf="0.0001" ft="2.0" softtype="2" sheartype="1" multiplecrackshear="" beta="0.01" vf="0.02" lf="0.012" df="4e-05" ef="20000.0" gfib="1000.0" tau_0="1.0" f="0.7" kfib="0.9" fsstype="0" fdamtype="0" fibertype="2" crackspacing="0.025"/>
      <frcfcm d="0.0" talpha="0.0" e="20000.0" n="0.2" gf="0.0001" ft="2.0" softtype="2" sheartype="1" multiplecrackshear="" beta="0.01" vf="0.02" lf="0.012" df="4e-05" ef="20000.0" gfib="1000.0" tau_0="1.0" f="0.7" kfib="0.9" fsstype="0" fibertype="2" crackspacing="0.025" fdamtype="2" gammacrack="0.5" iterlimitglobal="50"/>
    </Materials>
    <BoundaryConditions>
      <!-- horizontal restraint -->
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="1"/>
      <!-- vertical restraint -->
      <BoundaryCondition loadtimefunction="2" dofs="2" values="0.0" set="2"/>
      <!-- lower right node - creating first crack -->
      <BoundaryCondition loadtimefunction="3" dofs="1" values="1e-05" set="3"/>
      <!-- upper right node - creating first crack and then shearing -->
      <BoundaryCondition loadtimefunction="4" dofs="1" values="1e-05" set="4"/>
      <!-- left upper node - shearing -->
      <BoundaryCondition id="5" loadtimefunction="5" dofs="1" values="1e-05" set="5"/>
      <!-- upper nodes - creating second crack -->
      <BoundaryCondition loadtimefunction="6" dofs="2" values="1e-05" set="6"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinfunction npoints="4" t="0.0 5.0 10.0 100.0" f_t_="0.0 2.0 2.0 2.0"/>
      <!-- 0  5 10 15 20 25 30 -->
      <PiecewiseLinfunction npoints="7" t="0.0 5.0 10.0 15.0 20.0 25.0 30.0" f_t_="0.0 2.0 4.0 2.0 2.0 4.0 2.0"/>
      <PiecewiseLinfunction id="5" npoints="7" t="0.0 5.0 10.0 15.0 20.0 25.0 30.0" f_t_="0.0 0.0 2.0 0.0 0.0 2.0 0.0"/>
      <PiecewiseLinfunction npoints="7" t="0.0 5.0 10.0 15.0 20.0 25.0 30.0" f_t_="0.0 0.0 0.0 0.0 3.0 3.0 3.0"/>
    </LoadTimeFunctions>
    <Sets>
      <!-- horizontal restraint -->
      <Set nodes="1 5 9"/>
      <!-- vertical restraint -->
      <Set nodes="1 2 5 6 9 10"/>
      <!-- lower right node - creating first crack -->
      <Set nodes="2 6 10"/>
      <!-- upper right node - creating first crack and then shearing -->
      <Set nodes="4 8 12"/>
      <!-- left upper node - shearing -->
      <Set id="5" nodes="3 7 11"/>
      <!-- upper nodes - creating second crack -->
      <Set nodes="3 4 7 8 11 12"/>
      <Set elementranges="1"/>
      <Set elementranges="2"/>
      <Set elementranges="3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-05">
      <ELEMENT tStep="11" number="1" gp="1" keyword="1" component="6" value="1.6442"/>
      <ELEMENT tStep="26" number="1" gp="1" keyword="1" component="6" value="0.85332"/>
      <ELEMENT tStep="11" number="2" gp="1" keyword="1" component="6" value="1.5767143"/>
      <ELEMENT tStep="26" number="2" gp="1" keyword="1" component="6" value="1.3593"/>
      <ELEMENT tStep="11" number="3" gp="1" keyword="1" component="6" value="1.442849"/>
      <ELEMENT tStep="26" number="3" gp="1" keyword="1" component="6" value="1.072458"/>
    </errorcheck>
  </ExportModules>
</oofem>
