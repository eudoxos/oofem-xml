<oofem version="1">
  <Output>eigen_beam2d.out</Output>
  <Description>Eigen vibration of simply suported beam</Description>
  <!--LinearStatic 1 nsteps 1-->
  <Analysis type="EigenValueDynamic" nroot="4" rtolv="1e-06"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 0.0 0.25"/>
      <Node coords="0.0 0.0 0.5"/>
      <Node coords="0.0 0.0 0.75"/>
      <Node id="5" coords="0.0 0.0 1.0"/>
      <Node coords="0.0 0.0 1.25"/>
      <Node coords="0.0 0.0 1.5"/>
      <Node coords="0.0 0.0 1.75"/>
      <Node coords="0.0 0.0 2.0"/>
      <Node id="10" coords="0.0 0.0 2.25"/>
      <Node coords="0.0 0.0 2.5"/>
      <Node coords="0.0 0.0 2.75"/>
      <Node coords="0.0 0.0 3.0"/>
      <Node coords="0.0 0.0 3.25"/>
      <Node id="15" coords="0.0 0.0 3.5"/>
      <Node coords="0.0 0.0 3.75"/>
      <Node coords="0.0 0.0 4.0"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2"/>
      <Beam2d nodes="2 3"/>
      <Beam2d nodes="3 4"/>
      <Beam2d nodes="4 5"/>
      <Beam2d id="5" nodes="5 6"/>
      <Beam2d nodes="6 7"/>
      <Beam2d nodes="7 8"/>
      <Beam2d nodes="8 9"/>
      <Beam2d nodes="9 10"/>
      <Beam2d id="10" nodes="10 11"/>
      <Beam2d nodes="11 12"/>
      <Beam2d nodes="12 13"/>
      <Beam2d nodes="13 14"/>
      <Beam2d nodes="14 15"/>
      <Beam2d id="15" nodes="15 16"/>
      <Beam2d nodes="16 17"/>
    </Elements>
    <CrossSections>
      <SimpleCS area="0.06" iy="0.00045" beamshearcoeff="1e+60" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="25.0" e="25000000.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-16"/>
      <Set nodes="1 17"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.001">
      <!--check eigen values-->
      <EIGVAL tStep="1" eignum="1" value="2853.78786" tolerance="0.001"/>
      <EIGVAL tStep="1" eignum="2" value="45662.0244" tolerance="0.02"/>
    </errorcheck>
  </ExportModules>
</oofem>
