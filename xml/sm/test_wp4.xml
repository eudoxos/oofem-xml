<oofem version="1">
  <Output>test_wp4.out</Output>
  <Description>Winkler pasternak model: test of settlement profile, quadratic triagle elements</Description>
  <Analysis type="linearstatic" nsteps="1"/>
  <!-- vtkxml tstep_step 1 domain_all primvars 1 1  vars 3 1 2 4 stype 2 -->
  <Domain domain="2dmindlinplate">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="3.0 1.0 0.0"/>
      <Node coords="9.0 0.0 0.0"/>
      <Node id="5" coords="9.0 4.0 0.0"/>
      <Node coords="0.0 2.0 0.0"/>
      <Node coords="1.5 0.5 0.0"/>
      <Node coords="1.5 2.5 0.0"/>
      <Node coords="4.5 0.0 0.0"/>
      <Node id="10" coords="4.5 4.0 0.0"/>
      <Node coords="6.0 0.5 0.0"/>
      <Node coords="6.0 2.5 0.0"/>
      <Node coords="9.0 2.0 0.0"/>
    </Nodes>
    <Elements>
      <Tria2PlateSubSoil nodes="1 3 2 7 8 6"/>
      <Tria2PlateSubSoil nodes="1 4 3 9 11 7"/>
      <Tria2PlateSubSoil nodes="2 3 5 8 12 10"/>
      <Tria2PlateSubSoil nodes="3 4 5 11 13 12"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <winklerpasternak d="2500.0" c1="1.0" c2="2.0"/>
    </Materials>
    <BoundaryConditions>
      <!-- BoundaryCondition  1 loadTimeFunction 1 dofs 1 3 values 1 0.1 set 2 -->
      <NodalLoad loadtimefunction="1" dofs="3" components="1.0" set="2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- 1.27024637e-01 -->
    <errorcheck tolerance="0.0005">
      <NODE tStep="1" number="1" dof="3" unknown="d" value="0.0402994362"/>
      <NODE tStep="1" number="2" dof="3" unknown="d" value="0.0189744851"/>
      <NODE tStep="1" number="3" dof="3" unknown="d" value="0.127024637"/>
    </errorcheck>
  </ExportModules>
</oofem>
