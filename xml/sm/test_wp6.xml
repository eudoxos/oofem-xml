<oofem version="1">
  <Output>test_wp6.out</Output>
  <Description>Winkler pasternak model: test of constant surface load in step 1 on quadratic elements. The same load applies in step 2, using equivalent nodal load instead.</Description>
  <Analysis type="StaticStructural" nsteps="2"/>
  <Domain domain="2dmindlinplate">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.15 0.0 0.0"/>
      <Node coords="0.3 0.0 0.0"/>
      <Node coords="0.55 0.0 0.0"/>
      <Node id="5" coords="0.8 0.0 0.0"/>
      <Node coords="0.0 0.5 0.0"/>
      <Node coords="0.15 0.5 0.0"/>
      <Node coords="0.3 0.5 0.0"/>
      <Node coords="0.55 0.5 0.0"/>
      <Node id="10" coords="0.8 0.5 0.0"/>
      <Node coords="0.0 0.25 0.0"/>
      <Node coords="0.3 0.25 0.0"/>
      <Node coords="0.55 0.25 0.0"/>
      <Node coords="0.8 0.25 0.0"/>
    </Nodes>
    <Elements>
      <quad2platesubsoil nodes="1 3 8 6 2 12 7 11"/>
      <Tria2PlateSubSoil nodes="3 5 8 4 13 12"/>
      <Tria2PlateSubSoil nodes="5 10 8 14 9 13"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <winklerpasternak d="2500.0" c1="3.0" c2="0.4"/>
    </Materials>
    <BoundaryConditions>
      <ConstantSurfaceLoad loadtimefunction="1" dofs="3" components="7.0" set="2"/>
      <!--Quadratic triangle - equivalent nodal load is just at mid-points of edges. Corver vertices have zero load.-->
      <NodalLoad loadtimefunction="2" dofs="3" components="0.29166666666" set="3"/>
      <!--Quadratic rectangle - equivalent nodal load at mid-nodes A*q/3 (total 4*A*q), at corner vertices -A*q/12 (total -A*q*3)-->
      <NodalLoad loadtimefunction="2" dofs="3" components="0.35" set="4"/>
      <NodalLoad loadtimefunction="2" dofs="3" components="-0.0875" set="5"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PeakFunction t="1.0" f_t_="1.0"/>
      <PeakFunction t="2.0" f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set allelements=""/>
      <Set elementboundaries="1 1 2 1"/>
      <Set nodes="4 13 12"/>
      <Set nodes="2 12 7 11"/>
      <Set id="5" nodes="1 3 8 6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0005">
      <NODE tStep="1" number="1" dof="3" unknown="d" value="1.941999"/>
      <NODE tStep="1" number="3" dof="3" unknown="d" value="1.79566126"/>
      <NODE tStep="1" number="4" dof="3" unknown="d" value="1.59531221"/>
      <NODE tStep="1" number="5" dof="3" unknown="d" value="1.38671935"/>
      <NODE tStep="1" number="8" dof="3" unknown="d" value="1.67031938"/>
      <NODE tStep="2" number="1" dof="3" unknown="d" value="1.941999"/>
      <NODE tStep="2" number="3" dof="3" unknown="d" value="1.79566126"/>
      <NODE tStep="2" number="4" dof="3" unknown="d" value="1.59531221"/>
      <NODE tStep="2" number="5" dof="3" unknown="d" value="1.38671935"/>
      <NODE tStep="2" number="8" dof="3" unknown="d" value="1.67031938"/>
    </errorcheck>
  </ExportModules>
</oofem>
