<oofem version="1">
  <Output>lattice3dbondplast2.out</Output>
  <Description>Direct Tension. 3D Bond Plasticity Model</Description>
  <Analysis type="NonLinearStatic" nsteps="1" contextoutputstep="1000">
    <Metasteps>
      <Metastep nsteps="3" rtolv="1e-06" stiffmode="1" controllmode="1" refloadmode="0" minsteplength="0.0001" ddm="2 1" ddv="-0.0001" ddltf="2"/>
    </Metasteps>
  </Analysis>
  <Domain domain="3dlattice">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0" bc="1 1 1 1 1 1"/>
      <Node coords="0.001 0.0 0.0" bc="0 0 1 1 1 1"/>
    </Nodes>
    <Elements>
      <lattice3D nodes="1 2" crosssect="1" mat="1" polycoords="5e-05 -0.05 -0.05 5e-05 0.05 -0.05 5e-05 0.05 0.05 5e-05 -0.05 0.05"/>
    </Elements>
    <CrossSections>
      <latticecs material="1"/>
    </CrossSections>
    <Materials>
      <latticebondplast d="0.0" talpha="0.0" e="40000000000.0" a1="1.0" a2="0.0" fc="130000000.0" angle1="0.2" iter="100" tol="1e-12" ef="0.01"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" prescribedvalue="0.0"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="2" t="0.0 3.0" f_t_="0.0 1.0"/>
    </LoadTimeFunctions>
    <Sets/>
  </Domain>
  <ExportModules>
    <!-- %BEGIN_CHECK% -->
    <!-- ELEMENT number 1 gp 1 record 0 keyword strains component 1 -->
    <!-- ELEMENT number 1 gp 1 record 1 keyword stresses component 1 -->
    <!-- %END_CHECK% -->
    <errorcheck>
      <ELEMENT tStep="1" number="1" gp="1" keyword="138" component="1" value="0.0" tolerance="1e-05"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="139" component="1" value="0.0" tolerance="1e-05"/>
      <ELEMENT tStep="2" number="1" gp="1" keyword="138" component="1" value="-0.0333333333" tolerance="1e-06"/>
      <ELEMENT tStep="2" number="1" gp="1" keyword="139" component="1" value="-795057558.0" tolerance="1.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
