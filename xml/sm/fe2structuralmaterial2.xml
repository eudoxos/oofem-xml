<oofem version="1">
  <Output>test.out</Output>
  <Description>Test for multiscale modeling using fe2structuralmaterial - plane stress.</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node coords="1.0 0.2 0.0"/>
      <Node coords="0.0 0.2 0.0"/>
      <Node id="5" coords="0.2 0.0 0.0"/>
      <Node coords="0.4 0.0 0.0"/>
      <Node coords="0.6 0.0 0.0"/>
      <Node coords="0.8 0.0 0.0"/>
      <Node coords="0.8 0.2 0.0"/>
      <Node id="10" coords="0.6 0.2 0.0"/>
      <Node coords="0.4 0.2 0.0"/>
      <Node coords="0.2 0.2 0.0"/>
    </Nodes>
    <Elements>
      <planestress2d id="13" nodes="1 5 12 4"/>
      <planestress2d nodes="5 6 11 12"/>
      <planestress2d id="15" nodes="6 7 10 11"/>
      <planestress2d nodes="7 8 9 10"/>
      <planestress2d nodes="8 2 3 9"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <!-- Linear elasticity -->
      <structfe2material d="1.0" filename="fe2structuralmaterial2.in.rve" use_num_tangent=""/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="0.0 -500000.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="13-17"/>
      <Set nodes="1 4"/>
      <Set nodes="2 3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!-- check selected nodes -->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="-0.000325"/>
      <NODE tStep="1" number="2" dof="2" unknown="d" value="-0.00220690476"/>
    </errorcheck>
  </ExportModules>
</oofem>
