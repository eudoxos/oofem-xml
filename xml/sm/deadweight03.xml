<oofem version="1">
  <Output>deadweight03.out</Output>
  <Description>Test of vertical deadweight on linear triagles</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="4.0 0.0 0.0"/>
      <Node coords="4.0 1.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
    </Nodes>
    <Elements>
      <TrPlaneStress2d nodes="1 2 3"/>
      <TrPlaneStress2d nodes="1 3 4"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.2" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="100.0" n="0.1" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
      <DeadWeight loadtimefunction="1" components="0.0 -10.0" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1 2"/>
      <Set nodes="1"/>
      <Set nodes="1 2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.001">
      <!-- check reactions -->
      <REACTION tStep="1" number="1" dof="2" value="4.0"/>
      <REACTION tStep="1" number="2" dof="2" value="4.0"/>
      <!-- check displacements of top nodes -->
      <NODE tStep="1" number="3" dof="2" unknown="d" value="-0.0666459054"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="-0.0333540946"/>
      <!-- check element No. 1 strain vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="-0.0666459054"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="2" value="-0.0333540946"/>
      <!-- check element No. 1 stress vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="-6.6701"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="2" value="-3.3299"/>
    </errorcheck>
  </ExportModules>
</oofem>
