<oofem version="1">
  <Output>plasticRemap1.out</Output>
  <Description>Test case for mapping of state variables between domains. This is a quite academic test case: at the end of each time step, the domain is cloned and the state variables are mapped to the new domain. Written by Erik Svenning, Chalmers University of Technology.</Description>
  <Analysis type="StaticStructural" nsteps="5" deltat="1.0" rtolf="1e-07" maxiter="25" miniter="2" manrmsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node coords="1.0 1.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2DXfem nodes="1 2 3 4" crosssect="1" nip="4" nlgeo="0" useplanestrain="1"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1"/>
    </CrossSections>
    <Materials>
      <!-- J2-plasticity -->
      <j2mat d="1.0" e="4000.0" n="0.3" ry="10.0" ihm="1.0" talpha="0.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="1"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.008" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PiecewiseLinFunction t="1.0 6.0" f_t_="0.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="1"/>
      <Set nodes="2"/>
      <Set nodes="3 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-07">
      <!-- step 2 -->
      <REACTION tStep="2" number="3" dof="2" value="3.51648352"/>
      <REACTION tStep="2" number="4" dof="2" value="3.51648352"/>
      <!-- step 3 -->
      <REACTION tStep="3" number="3" dof="2" value="5.68173688"/>
      <REACTION tStep="3" number="4" dof="2" value="5.68173688"/>
      <!-- step 4 -->
      <REACTION tStep="4" number="3" dof="2" value="5.74281407"/>
      <REACTION tStep="4" number="4" dof="2" value="5.74281407"/>
      <!-- step 5 -->
      <REACTION tStep="5" number="3" dof="2" value="5.76502539"/>
      <REACTION tStep="5" number="4" dof="2" value="5.76502539"/>
    </errorcheck>
  </ExportModules>
</oofem>
