<oofem version="1">
  <Output>trussb3_relax.out</Output>
  <Description>Test of 1D relaxation behaviour using Truss2d elements and B3 material model</Description>
  <!--example from dissertation-->
  <Analysis type="StaticStructural" nsteps="30" prescribedtimes="0.1 0.1373 0.1887 0.2592 0.3562 0.4893 0.6723 0.9236 1.2689 1.7433 2.395 3.2903 4.5203 6.2101 8.5316 11.721 16.1026 22.1221 30.3919 41.7531 57.3615 78.8046 108.2636 148.7352 204.3359 280.7216 385.662 529.8317 727.8954 1000.0"/>
  <Domain domain="2dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 0.0 1.0"/>
      <Node coords="0.0 0.0 2.0"/>
    </Nodes>
    <Elements>
      <Truss2d nodes="1 2"/>
      <Truss2d nodes="2 3"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" width="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <!--CebFip78 1 d 0. E28 36.e3 relMatAge 3. n 0.2 nChainUnits 4 fibf 3.8 kap_a 3.0 kap_c 1.0 kap_tt 0.633 u 3.0-->
      <!--DoublePowerLaw 1 relMatAge 30. E28 45625.868 fi1 3.538 m 0.305 n 0.166 alpha 0.081-->
      <B3Mat relmatage="28.0" fc="27.58" cc="219.45" w_c="0.6" a_c="7.0" t0="28.0" shmode="0" d="0.0" n="0.18" timefactor="1.0" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="3"/>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.002" set="4"/>
      <NodalLoad loadtimefunction="1" dofs="1 3" components="0.0 3.0" set="0"/>
      <NodalLoad id="5" loadtimefunction="2" dofs="1 3" components="0.0 -3.0" set="0"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <HeavisideLTF origin="0.0" value="1.0"/>
      <HeavisideLTF origin="400.0" value="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1"/>
      <Set nodes="2 3"/>
      <Set nodes="3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-12">
      <!--check nodal values at the end of time interest-->
      <NODE tStep="1000" number="2" dof="3" unknown="d" value="0.001"/>
      <NODE tStep="1000" number="3" dof="3" unknown="d" value="0.002"/>
      <!--check element no. 1 strain vector-->
      <ELEMENT tStep="1000" number="1" gp="1" keyword="4" component="1" value="0.001"/>
      <ELEMENT tStep="1000" number="1" gp="1" keyword="1" component="1" value="11.055"/>
      <ELEMENT tStep="1000" number="2" gp="1" keyword="4" component="1" value="0.001"/>
      <ELEMENT tStep="1000" number="2" gp="1" keyword="1" component="1" value="11.055"/>
    </errorcheck>
  </ExportModules>
</oofem>
