<oofem version="1">
  <Output>test1.out</Output>
  <Description>Patch test of Truss1d elements -&gt; temperature load</Description>
  <Analysis type="StaticStructural" nsteps="1" stiffmode="0" manrmsteps="5"/>
  <Domain domain="1dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0"/>
      <Node coords="2.0"/>
    </Nodes>
    <Elements>
      <Truss1d nodes="1 2"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.1" width="10.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <isoasymm1d et="1.0" ec="15.0" talpha="1.2e-05" m="200.0" d="1.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <NodalLoad loadtimefunction="1" components="-2.0" dofs="1" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-08">
      <REACTION tStep="1" number="1" dof="1" value="2.0"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="-0.269901345"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="-0.134950677"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-2.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
