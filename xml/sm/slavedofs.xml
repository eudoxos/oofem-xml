<oofem version="1">
  <Output>slavedofs.out</Output>
  <Description>Patch test of Truss2d elements -&gt; simple structure with slave dofs</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="8.0 0.0 0.0"/>
      <Node coords="2.0 0.0 3.0"/>
      <Node coords="0.0 0.0 0.0" dofidmask="1 3 5" mastermask="1 1 0" doftype="1 1 0"/>
      <Node id="5" coords="8.0 0.0 0.0" dofidmask="1 3 5" mastermask="2 2 0" doftype="1 1 0"/>
      <Node coords="2.0 0.0 3.0" dofidmask="1 3 5" mastermask="3 3 0" doftype="1 1 0"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2"/>
      <Beam2d nodes="4 3"/>
      <Beam2d nodes="5 6"/>
    </Elements>
    <CrossSections>
      <SimpleCS area="1.0" iy="0.0039366" beamshearcoeff="1e+18" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="1.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.0" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="1 3 5" components="0.0 -1.0 0.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-3"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
      <Set nodes="3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!-- check reactions -->
      <REACTION tStep="1" number="1" dof="1" value="0.0"/>
      <REACTION tStep="1" number="1" dof="3" value="0.75"/>
      <REACTION tStep="1" number="2" dof="3" value="0.25"/>
      <!-- check all nodes -->
      <!-- check element stress vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="7" component="1" value="0.5"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="7" component="1" value="-0.9013878"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="7" component="1" value="-0.559017"/>
    </errorcheck>
  </ExportModules>
</oofem>
