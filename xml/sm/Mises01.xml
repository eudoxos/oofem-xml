<oofem version="1">
  <Output>Mises01.out</Output>
  <Description>Test of Mises plasticity model with linear hardening/softening</Description>
  <Analysis type="StaticStructural" nsteps="20" rtolf="0.0001" maxiter="20"/>
  <Domain domain="1dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.5 0.0 0.0"/>
    </Nodes>
    <Elements>
      <truss1d nodes="1 2"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" width="10.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <MisesMat d="1.0" talpha="1.2e-05" e="1.0" n="0.2" sig0="1" h="0.1" omega_crit="0.1" a="0.1"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="2" dofs="1" values="0.4" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinFunction t="1.0 6.0 11.0 16.0 21.0" f_t_="0.0 5.0 -5.0 5.0 -5.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- Used for Extractor -->
    <errorcheck tolerance="0.0001">
      <ELEMENT tStep="2" number="1" gp="1" keyword="4" component="1" value="0.8"/>
      <ELEMENT tStep="2" number="1" gp="1" keyword="1" component="1" value="0.8"/>
      <ELEMENT tStep="6" number="1" gp="1" keyword="4" component="1" value="4.0"/>
      <ELEMENT tStep="6" number="1" gp="1" keyword="1" component="1" value="1.2423"/>
      <ELEMENT tStep="20" number="1" gp="1" keyword="4" component="1" value="-2.4"/>
      <ELEMENT tStep="20" number="1" gp="1" keyword="1" component="1" value="-2.1853"/>
    </errorcheck>
  </ExportModules>
</oofem>
