<oofem version="1">
  <Output>axisymm06.out</Output>
  <Description>Test of linear axisymmetric triangular elements, a thin ring loaded only in radial direction (y direction)</Description>
  <Analysis type="LinearStatic" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="4.0 0.0"/>
      <Node coords="4.0 2.0"/>
      <Node coords="7.0 0.0"/>
      <Node coords="7.0 2.0"/>
      <Node id="5" coords="10.0 0.0"/>
      <Node coords="10.0 2.0"/>
    </Nodes>
    <Elements>
      <Axisymm3d nodes="1 4 2" boundaryloads="2 2"/>
      <Axisymm3d nodes="1 3 4"/>
      <Axisymm3d nodes="3 6 4" boundaryloads="2 2"/>
      <Axisymm3d nodes="3 5 6"/>
    </Elements>
    <CrossSections>
      <SimpleCS material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="1000.0" n="0.0" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="2"/>
      <ConstantEdgeLoad loadtimefunction="1" components="0.0 10.0" loadtype="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="1 3 5"/>
      <Set elementboundaries="1 2 3 2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-06">
      <!-- Exact displacement is 2.0e-2 -->
      <NODE tStep="1" number="2" dof="2" unknown="d" value="0.0219036007"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="0.0199832366"/>
      <NODE tStep="1" number="6" dof="2" unknown="d" value="0.0189685205"/>
      <!-- Constant strains and stresses over an element -->
      <!-- Element 1 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="-0.00010412"/>
      <!-- Exact solution for is 1.0e-1 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="0.010952"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="3" value="2.1648e-05"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="-0.0001446"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-0.10412"/>
      <!-- Exact solution is sig_y=10.0 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="10.9518003"/>
      <!-- Element 4 -->
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="1" value="4.5464e-05"/>
      <!-- Exact solution for is 1.0e-1 -->
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="2" value="0.0094843"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="3" value="-5.1712e-06"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="6" value="5.9853e-05"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="1" value="0.045464"/>
      <!-- Exact solution is sig_y=10.0 -->
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="2" value="9.48426023"/>
    </errorcheck>
  </ExportModules>
</oofem>
