<oofem version="1">
  <Output>beam2d_1.out</Output>
  <Description>Homework www sm40 no. 1</Description>
  <!--only momentum influence to the displacements is taken into account-->
  <!--beamShearCoeff is artificially enlarged.-->
  <Analysis type="StaticStructural" nsteps="3"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="2.4 0.0 0.0"/>
      <Node coords="3.8 0.0 0.0"/>
      <Node coords="5.8 0.0 1.5"/>
      <Node id="5" coords="7.8 0.0 3.0"/>
      <Node coords="2.4 0.0 3.0"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2"/>
      <Beam2d nodes="2 3" dofstocondense="6"/>
      <Beam2d nodes="3 4" dofstocondense="3"/>
      <Beam2d nodes="4 5"/>
      <Beam2d id="5" nodes="6 2" dofstocondense="6"/>
    </Elements>
    <CrossSections>
      <SimpleCS area="100000000.0" iy="0.0039366" beamshearcoeff="1e+18" thick="0.54" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="30000000.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.0" set="4"/>
      <BoundaryCondition loadtimefunction="1" dofs="5" values="0.0" set="5"/>
      <BoundaryCondition loadtimefunction="2" dofs="1 3 5" values="0.0 0.0 -6e-06" set="6"/>
      <ConstantEdgeLoad loadtimefunction="1" components="0.0 10.0 0.0" loadtype="3" set="3"/>
      <NodalLoad id="5" loadtimefunction="1" dofs="1 3 5" components="-18.0 24.0 0.0" set="2"/>
      <StructTemperatureLoad loadtimefunction="3" components="30.0 -20.0" set="7"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PeakFunction t="1.0" f_t_="1.0"/>
      <PeakFunction t="2.0" f_t_="1.0"/>
      <PeakFunction t="3.0" f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-5"/>
      <Set nodes="4"/>
      <Set elementedges="1 1"/>
      <Set nodes="1 5"/>
      <Set id="5" nodes="3"/>
      <Set nodes="6"/>
      <Set elements="1 2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!--exact solution-->
    <errorcheck tolerance="0.005">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="3" value="-8.9376" tolerance="0.0001"/>
      <REACTION tStep="1" number="3" dof="5" value="0.0" tolerance="0.0001"/>
      <REACTION tStep="1" number="5" dof="3" value="-18.75" tolerance="0.001"/>
      <REACTION tStep="1" number="6" dof="1" value="18.0" tolerance="0.001"/>
      <REACTION tStep="1" number="6" dof="3" value="-20.312" tolerance="0.001"/>
      <REACTION tStep="1" number="6" dof="5" value="-54.002" tolerance="0.005"/>
      <!--check all nodes-->
      <NODE tStep="1" number="4" dof="1" unknown="d" value="-0.00175287942"/>
      <NODE tStep="2" number="4" dof="1" unknown="d" value="1.79999937e-05"/>
      <!--check element no. 1 force vector-->
      <!--ELEMENT tStep 1 number 1 gp 1 keyword 7 component 6  value -7.3498e+00 tolerance 1.e-2-->
      <!--ELEMENT tStep 1 number 1 gp 1 keyword 7 component 5  value -1.5062e+01 tolerance 2.e-3-->
      <NODE tStep="3" number="4" dof="1" unknown="d" value="0.000947323653"/>
    </errorcheck>
  </ExportModules>
</oofem>
