<oofem version="1">
  <Output>patch130.out</Output>
  <Description>Patch test of TrPlaneStress2d elements -&gt; pure compression</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="3.0 1.0 0.0"/>
      <Node coords="9.0 0.0 0.0"/>
      <Node id="5" coords="9.0 4.0 0.0"/>
    </Nodes>
    <Elements>
      <TrPlaneStress2d nodes="1 3 2"/>
      <TrPlaneStress2d nodes="1 4 3"/>
      <TrPlaneStress2d nodes="3 5 2"/>
      <TrPlaneStress2d nodes="3 4 5"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.15" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="0.0" e="15.0" n="0.25" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="1"/>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="-2.5 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="1 2"/>
      <Set nodes="4 5"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- exact solution -->
    <!-- DISPLACEMENT                   STRAIN                     STRESS -->
    <!-- node 1   0.0                epsilon_x = -0.520833333   sigma_x = -8.333333333 -->
    <!-- node 2   0.0                epsilon_y =  0.0           sigma_y = -2.083333333 -->
    <!-- node 3  -1.5625             gama_xy   =  0.0           tau_xy  =  0.0 -->
    <!-- node 4  -4.6875 -->
    <!-- node 5  -4.6875 -->
    <!-- REACTION -->
    <!-- node 1   R_u = 2.5   R_v =  1.40625 -->
    <!-- node 2   R_u = 2.5   R_v = -1.40625 -->
    <!-- node 4   R_u = 0.0   R_v =  1.40625 -->
    <!-- node 5   R_u = 0.0   R_v = -1.40625 -->
    <errorcheck tolerance="0.0001">
      <!-- check reactions -->
      <REACTION tStep="1" number="1" dof="1" value="2.5"/>
      <REACTION tStep="1" number="1" dof="2" value="1.40625"/>
      <REACTION tStep="1" number="2" dof="1" value="2.5"/>
      <REACTION tStep="1" number="2" dof="2" value="-1.40625"/>
      <REACTION tStep="1" number="4" dof="2" value="1.40625"/>
      <REACTION tStep="1" number="5" dof="2" value="-1.40625"/>
      <!-- check all nodes -->
      <NODE tStep="1" number="3" dof="1" unknown="d" value="-1.5625"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="-4.6875"/>
      <NODE tStep="1" number="5" dof="1" unknown="d" value="-4.6875"/>
      <!-- check element no. 1 strain vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="-0.52083"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="0.0"/>
      <!-- check element no. 1 stress vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-8.333333333"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="-2.083333333"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="1" value="-0.520833333"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="1" value="-8.333333333"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="2" value="-2.083333333"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="1" value="-0.520833333"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="1" value="-8.333333333"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="2" value="-2.083333333"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="1" value="-0.520833333"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="1" value="-8.333333333"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="2" value="-2.083333333"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="6" value="0.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
