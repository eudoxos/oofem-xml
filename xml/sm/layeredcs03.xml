<oofem version="1">
  <Output>layeredcs03.out</Output>
  <Description>Test of Beam2d elements -&gt; cantilever loaded by end moment, comparing elastic integral and layered models</Description>
  <Analysis type="StaticStructural" nsteps="2"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="3.0 0.0 0.0"/>
      <Node id="11" coords="0.0 0.0 0.0"/>
      <Node coords="3.0 0.0 0.0"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2"/>
      <Beam2d id="11" nodes="11 12"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.3" width="1.0" area="0.3" iy="0.00225" beamshearcoeff="1.0" material="1" set="1"/>
      <LayeredCS nlayers="2" layermaterials="1 1" thicks="0.15 0.15" widths="1.0 1.0" midsurf="0.15" nintegrationpoints="2" set="2"/>
    </CrossSections>
    <Materials>
      <IsoLE d="0.0" e="15000000.0" n="0.25" talpha="0.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3 5" values="0.0 0.0 0.0" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="3" components="0.1" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
      <Set elements="11"/>
      <Set nodes="1 11"/>
      <Set nodes="2 12"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- beam exact solution: -->
    <!-- load-level 1.0 -> end of elastic behaviour and begining of elasto-  -->
    <!-- plastic behaviour -->
    <!-- at load-level 1.5 -> vycerpana unosnost -kolaps. -->
    <errorcheck tolerance="1e-12">
      <!-- check load levels -->
      <!-- check node displacement at final step -->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="3" unknown="d" value="2.68333333e-05"/>
      <NODE tStep="1" number="2" dof="5" unknown="d" value="-1.33333333e-05"/>
      <NODE tStep="1" number="12" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="12" dof="3" unknown="d" value="2.68333333e-05"/>
      <NODE tStep="1" number="12" dof="5" unknown="d" value="-1.33333333e-05"/>
      <!-- check elements -->
      <REACTION tStep="1" number="1" dof="3" value="-0.1"/>
      <REACTION tStep="1" number="1" dof="5" value="0.3"/>
      <REACTION tStep="1" number="11" dof="3" value="-0.1"/>
      <REACTION tStep="1" number="11" dof="5" value="0.3"/>
    </errorcheck>
  </ExportModules>
</oofem>
