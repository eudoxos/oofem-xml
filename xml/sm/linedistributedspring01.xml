<oofem version="1">
  <Output>linedistributedspring01.out</Output>
  <Description>Test of linedistributedspring element</Description>
  <Analysis type="LinearStatic" nsteps="1"/>
  <Domain domain="3d">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 3.0 0.0"/>
    </Nodes>
    <Elements>
      <linedistributedspring nodes="1 2" dofs="1 2 3" k="1.0 2.0 3.0"/>
    </Elements>
    <CrossSections>
      <simplecs set="3"/>
    </CrossSections>
    <Materials/>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2 3" values="0.0 0.0 0.0" set="1"/>
      <!--BoundaryCondition 2 loadTimeFunction 1 dofs 3 1 2 3 values 3 0 1 2 set 2-->
      <NodalLoad loadtimefunction="1" dofs="1 2 3" components="0.0 1.0 2.0" set="2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="1"/>
      <Set nodes="2"/>
      <Set elements="1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-05">
      <!--Note: this is the exact solution-->
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="0.0"/>
      <REACTION tStep="1" number="1" dof="2" value="0.5"/>
      <REACTION tStep="1" number="1" dof="3" value="1.0"/>
      <!--check all nodes-->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="2" unknown="d" value="0.5"/>
      <NODE tStep="1" number="2" dof="3" unknown="d" value="0.66666666667"/>
    </errorcheck>
  </ExportModules>
</oofem>
