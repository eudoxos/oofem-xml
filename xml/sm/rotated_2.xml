<oofem version="1">
  <Output>rotated_2.out</Output>
  <Description>Patch test of Truss2d elements -&gt; simple structure with rotated support and non nodal loading</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="5.0 0.0 0.0" lcs="0.8660254 0.0 0.5 0.0 1.0 0.0"/>
      <Node coords="0.0 0.0 10.0"/>
    </Nodes>
    <Elements>
      <Truss2d nodes="1 2"/>
      <Truss2d nodes="1 3"/>
      <Truss2d nodes="2 3" boundaryloads="3 1"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.1" width="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="1.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.0" set="3"/>
      <LinearEdgeLoad loadtimefunction="1" dofs="1 3" components="1.0 0.0 1.0 0.0" loadtype="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-3"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
      <Set elementedges="3 1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="-4.7254"/>
      <REACTION tStep="1" number="1" dof="3" value="-11.1803399"/>
      <REACTION tStep="1" number="2" dof="3" value="12.91"/>
      <!--check all nodes-->
      <!--check element  strain vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="47.2536762"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="1" value="111.803399"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="1" value="-125.0"/>
      <!--check element stress vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="47.2536762"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="1" value="111.803399"/>
