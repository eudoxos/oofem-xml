<oofem version="1">
  <Output>xfemMultipleCracks1.out</Output>
  <Description>XFEM simulation: A single element with three cracks and cohesive zones, loaded in shear. Written by Erik Svenning, Chalmers University of Technology.</Description>
  <Analysis type="StaticStructural" nsteps="5" deltat="1.0" rtolf="1e-06" maxiter="25" miniter="2" lstype="0" smtype="1" manrmsteps="1"/>
  <!-- vtkxml tstep_all domain_all primvars 1 1 cellvars 1 1 -->
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0"/>
      <Node coords="1.0 0.0"/>
      <Node coords="1.0 1.0"/>
      <Node coords="0.0 1.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2DXfem nodes="1 2 3 4" nip="4" nlgeo="0" czmaterial="2"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <!-- Linear elasticity -->
      <IsoLE d="0.0" e="100000.0" n="0.0" talpha="0.0"/>
      <!-- Bilinear cohesive zone material -->
      <intmatbilinearcz kn="100000.0" g1c="1.0" g2c="1.0" mu="0.0" gamma="0.5" sigf="100.0"/>
    </Materials>
    <BoundaryConditions>
      <!-- intmatbilinearcz 2 kn 1.0e6 g1c 1.0e4 g2c 1.5e4 mu 0.0 gamma 0.5 sigf 1.e4 -->
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.02 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PiecewiseLinFunction t="-1.0 1.0 6.0" f_t_="0.0 0.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1 2 4"/>
      <Set nodes="3"/>
    </Sets>
    <XFemManager type="XfemManager" numberofgppertri="1" vtkexport="1" exportfields="2 3 4">
      <EnrichmentItems>
        <!-- DiscontinuousFunction 1 -->
        <!-- polygoncrack 1 points 6 -0.1 0.3 0.5 0.3 1.1 0.3 -->
        <!-- Crack 1 -->
        <crack>
          <EnrichmentFunction type="HeavisideFunction"/>
          <!-- DiscontinuousFunction 1 -->
          <EnrichmentGeometry type="PolygonLine" points="-0.5 0.3 -0.2 0.3 1.1 0.3"/>
        </crack>
        <!-- Crack 2 -->
        <crack>
          <EnrichmentFunction type="HeavisideFunction"/>
          <!-- DiscontinuousFunction 1 -->
          <EnrichmentGeometry id="2" type="PolygonLine" points="0.5 1.1 0.5 0.5 0.5 -0.1"/>
        </crack>
        <!-- Crack 3 -->
        <crack>
          <EnrichmentFunction type="HeavisideFunction"/>
          <!-- DiscontinuousFunction 1 -->
          <EnrichmentGeometry id="2" type="PolygonLine" points="-0.1 0.8 0.4 0.5 0.7 -0.1 0.8 -0.2"/>
        </crack>
      </EnrichmentItems>
    </XFemManager>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.001">
      <!-- step 2 -->
      <REACTION tStep="2" number="3" dof="1" value="50.1446686"/>
      <REACTION tStep="2" number="4" dof="1" value="-23.6298729"/>
      <!-- step 3 -->
      <REACTION tStep="3" number="3" dof="1" value="48.421162"/>
      <REACTION tStep="3" number="4" dof="1" value="-21.2269928"/>
      <!-- step 4 -->
      <REACTION tStep="4" number="3" dof="1" value="40.6049756"/>
      <REACTION tStep="4" number="4" dof="1" value="-17.540408"/>
    </errorcheck>
  </ExportModules>
</oofem>
