<oofem version="1">
  <Output>materOrient01.out</Output>
  <Description>test of local coordinate system with material orientation on element</Description>
  <Analysis type="StaticStructural" nsteps="1" rtolf="1e-05" deltat="1.0"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0"/>
      <Node coords="4.0 0.0"/>
      <Node coords="4.0 2.0"/>
      <Node coords="0.0 2.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2d nodes="1 2 3 4" lcs="0.0 1.0 0.0 1.0 0.0 0.0"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <orthole d="0.0" ex="2.0" ey="10.0" ez="5.0" nyyz="0.2" nyxz="0.3" nyxy="0.4" gyz="5.0" gxz="2.0" gxy="1.0" talphax="0.0" talphay="0.0" talphaz="0.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="1.0 0.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1 4"/>
      <Set nodes="1 2"/>
      <Set nodes="2 3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!-- check element no. 1 strain vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.1"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="-0.2"/>
      <!-- check element no. 1 stress vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="1.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
