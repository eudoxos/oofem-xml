<oofem version="1">
  <Output>eigenstrain01.out</Output>
  <Description>Test of Truss1d elements with prescribed eigenstrain load</Description>
  <Analysis type="LinearStatic" nsteps="1"/>
  <Domain domain="1dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0"/>
      <Node coords="2.0"/>
      <Node coords="4.0"/>
    </Nodes>
    <Elements>
      <Truss1d nodes="1 2"/>
      <Truss1d nodes="2 3"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.1" width="10.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE talpha="1.2e-05" d="1.0" e="2.0" n="0.2"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="3"/>
      <StructEigenstrainLoad loadtimefunction="1" components="0.1" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2"/>
      <Set nodes="1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-08">
      <!-- check all nodes -->
      <NODE tStep="1" number="1" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.2"/>
      <!-- check element strain vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="1" value="0.1"/>
      <!-- check element stress vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-0.2"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="1" value="0.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
