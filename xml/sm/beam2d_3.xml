<oofem version="1">
  <Output>beam2d_3.out</Output>
  <Description>Homework www sm40 no. 8</Description>
  <!--only momentum influence to the displacements is taken into account-->
  <!--beamShearCoeff is artificially enlarged.-->
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 4.7"/>
      <Node coords="0.0 0.0 2.7"/>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node id="5" coords="3.6 0.0 0.0"/>
      <Node coords="3.6 0.0 4.7"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2"/>
      <Beam2d nodes="2 3"/>
      <Beam2d nodes="3 4"/>
      <Beam2d nodes="4 5" dofstocondense="6"/>
      <Beam2d id="5" nodes="5 6" dofstocondense="3"/>
    </Elements>
    <CrossSections>
      <SimpleCS area="1000.0" iy="0.0026244" beamshearcoeff="1e+18" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="25000000.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3 5" values="0.0 0.0 0.0" set="3"/>
      <BoundaryCondition loadtimefunction="1" dofs="5" values="0.0" set="5"/>
      <BoundaryCondition loadtimefunction="1" dofs="1 3 5" values="0.0 0.005 0.0" set="6"/>
      <ConstantEdgeLoad loadtimefunction="1" dofs="1 3 5" components="0.0 5.0 0.0" loadtype="3" cstype="1" set="2"/>
      <NodalLoad id="5" loadtimefunction="1" dofs="1 3 5" components="0.0 10.0 0.0" set="4"/>
      <NodalLoad loadtimefunction="1" dofs="1 3 5" components="7.0 0.0 0.0" set="5"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-5"/>
      <Set elementedges="1 1 2 1"/>
      <Set nodes="1"/>
      <Set nodes="4"/>
      <Set id="5" nodes="5"/>
      <Set nodes="6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!--exact solution-->
    <errorcheck tolerance="1e-05">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="-21.74224"/>
      <REACTION tStep="1" number="1" dof="3" value="-8.704485"/>
      <REACTION tStep="1" number="1" dof="5" value="52.29966"/>
      <REACTION tStep="1" number="5" dof="5" value="0.0"/>
      <REACTION tStep="1" number="6" dof="1" value="-8.757762"/>
      <REACTION tStep="1" number="6" dof="3" value="-1.295515"/>
      <REACTION tStep="1" number="6" dof="5" value="41.16148"/>
      <!--check all nodes-->
      <NODE tStep="1" number="3" dof="5" unknown="d" value="-0.00140503831"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.00461950515"/>
      <!--check element no. 1 force vector-->
      <BEAM_ELEMENT tStep="1" number="1" keyword="1" component="1" value="8.70448469"/>
      <BEAM_ELEMENT tStep="1" number="1" keyword="1" component="2" value="-21.7422378"/>
      <BEAM_ELEMENT tStep="1" number="1" keyword="1" component="3" value="52.2996624"/>
      <!--element 5-->
      <BEAM_ELEMENT tStep="1" number="5" keyword="1" component="1" value="1.29551531"/>
      <BEAM_ELEMENT tStep="1" number="5" keyword="1" component="2" value="-8.75776225"/>
      <BEAM_ELEMENT tStep="1" number="5" keyword="1" component="3" value="0.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
