<oofem version="1">
  <Output>brick_nlgeo_1.out</Output>
  <Description>test of Brick elements with nlgeo 1(strain is the Green-Lagrangian strain) rotated as a rigid body</Description>
  <Analysis type="StaticStructural" nsteps="5"/>
  <Domain domain="3d">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node coords="1.0 1.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
      <Node id="5" coords="0.0 0.0 1.0"/>
      <Node coords="1.0 0.0 1.0"/>
      <Node coords="1.0 1.0 1.0"/>
      <Node coords="0.0 1.0 1.0"/>
    </Nodes>
    <Elements>
      <lspace nodes="1 2 3 4 5 6 7 8" nlgeo="1"/>
    </Elements>
    <CrossSections>
      <SimpleCS material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="0.0" e="15.0" n="0.25" talpha="1.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2 3" values="0.0 0.0 0.0" set="0"/>
      <BoundaryCondition loadtimefunction="1" dofs="1 2 3" values="0.5 0.5 0.5" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PiecewiseLinFunction t="1.0 1001.0" f_t_="0.0 1000.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-12">
      <!-- check Green-Lagrange strain tensor -->
      <ELEMENT tStep="5" number="1" gp="1" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="5" number="1" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="5" number="1" gp="1" keyword="4" component="3" value="0.0"/>
