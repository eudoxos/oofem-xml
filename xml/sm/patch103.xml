<oofem version="1">
  <Output>patch103.out</Output>
  <Description>Patch test of PlaneStress2d elements -&gt; thermal load</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="3.0 0.0 0.0"/>
      <Node coords="3.0 4.0 0.0"/>
      <Node id="5" coords="6.0 0.0 0.0"/>
      <Node coords="6.0 4.0 0.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2d nodes="4 2 1 3"/>
      <PlaneStress2d nodes="6 4 3 5"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.15" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE talpha="1.2e-05" d="1.0" e="15.0" n="0.25"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
      <StructTemperatureLoad loadtimefunction="1" components="-30.0 0.0" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2 5 6"/>
      <Set nodes="3 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-10">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="-0.00216"/>
      <REACTION tStep="1" number="1" dof="2" value="-0.00162"/>
      <REACTION tStep="1" number="2" dof="1" value="-0.00216"/>
      <REACTION tStep="1" number="2" dof="2" value="0.00162"/>
      <REACTION tStep="1" number="3" dof="2" value="-0.00324"/>
      <REACTION tStep="1" number="4" dof="2" value="0.00324"/>
      <REACTION tStep="1" number="5" dof="2" value="-0.00162"/>
      <REACTION tStep="1" number="6" dof="1" value="0.00216"/>
      <REACTION tStep="1" number="6" dof="2" value="0.00162"/>
      <!--check all nodes-->
      <NODE tStep="1" number="1" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="5" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="6" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="1" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="5" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="6" dof="2" unknown="d" value="0.0"/>
      <!--check element strain vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="3" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="3" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="3" keyword="4" component="6" value="0.0"/>
      <!--check element stress vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="0.0072"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="0.0072"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="1" value="0.0072"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="2" value="0.0072"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="6" value="0.0"/>
    </errorcheck>
    <hom tstep_all="" ists="1 4" strain_energy=""/>
  </ExportModules>
</oofem>
