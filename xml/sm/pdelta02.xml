<oofem version="1">
  <Output>pdelta02.out</Output>
  <Description>pdelta analysis of simple cantilever beam</Description>
  <!--only momentum influence to the displacements is taken into account-->
  <!--beamShearCoeff is artificially enlarged.-->
  <Analysis type="pdelta" nsteps="1" rtolv="0.001" maxiter="1"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 0.0 10.0"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2"/>
    </Elements>
    <CrossSections>
      <SimpleCS area="100000000.0" iy="8.33e-06" beamshearcoeff="1e+18" thick="0.54" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="205000000000.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3 5" values="0.0 0.0 0.0" set="2"/>
      <NodalLoad loadtimefunction="1" dofs="1 3 5" components="2000.0 -50000.0 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PeakFunction t="1.0" f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elements="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!--exact solution-->
    <errorcheck tolerance="0.005">
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.771430584"/>
      <REACTION tStep="1" number="1" dof="3" value="50000.0"/>
      <REACTION tStep="1" number="1" dof="1" value="-3952.0"/>
      <REACTION tStep="1" number="1" dof="5" value="-39520.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
