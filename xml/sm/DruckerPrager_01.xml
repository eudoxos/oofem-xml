<oofem version="1">
  <Output>DruckerPrager_01.out</Output>
  <Description>Test of DruckerPrager material under plane-strain conditions</Description>
  <Analysis type="StaticStructural" nsteps="10" rtolf="1e-06" maxiter="100"/>
  <!-- vtkxml tstep_step 1 domain_all primvars 1 1 vars 3 1 4 27 stype 1 -->
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="4.0 0.0 0.0"/>
      <Node coords="4.0 2.0 0.0"/>
      <Node coords="0.0 2.0 0.0"/>
    </Nodes>
    <Elements>
      <Quad1PlaneStrain nodes="1 2 3 4"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.3" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <DruckerPrager d="1.0" talpha="1.2e-05" e="30000.0" n="0.25" alpha="0.3" alphapsi="0.3" ht="1" iys="8.0" hm="1e-06"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="2" dofs="1" values="0.0004" set="3"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinFunction t="1.0 101.0" f_t_="0.0 100.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1 4"/>
      <Set nodes="2 3"/>
      <Set nodes="1 2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- Used for Extractor -->
    <errorcheck tolerance="1e-06">
      <ELEMENT tStep="2" number="1" gp="1" keyword="4" component="1" value="0.0001"/>
      <ELEMENT tStep="2" number="1" gp="1" keyword="1" component="1" value="3.2"/>
      <ELEMENT tStep="6" number="1" gp="1" keyword="4" component="1" value="0.0005"/>
      <ELEMENT tStep="6" number="1" gp="1" keyword="1" component="1" value="9.032799"/>
      <ELEMENT tStep="10" number="1" gp="1" keyword="4" component="1" value="0.0009"/>
      <ELEMENT tStep="10" number="1" gp="1" keyword="1" component="1" value="9.095706"/>
    </errorcheck>
  </ExportModules>
</oofem>
