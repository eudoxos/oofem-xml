<oofem version="1">
  <Output>axisymm02.out</Output>
  <Description>Test of linear axisymmetric quadrilateral elements, thick ring with internal overpressure, test based on lectures Advanced Finite Elements for Solids, Plates and Shells from Felippa, Colorado, Chapter 6, version 2013, pp. 6-11. Exact solution is on page 7-4.</Description>
  <!-- Exact solution for radial displacement u_r = (100+r^2)/(525*r) -->
  <!-- Exact solution for radial stress sig_r = 40./21.*(1-100./r^2) -->
  <!-- Exact solution for hoop stress sig_theta = 40./21.*(1+100./r^2) -->
  <Analysis type="LinearStatic" nsteps="1"/>
  <!-- vtkxml tstep_step 1 cellvars 1 46 vars 3 1 2 4 primvars 1 1 stype 2 -->
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="4.0 0.0"/>
      <Node coords="4.0 2.0"/>
      <Node coords="7.0 0.0"/>
      <Node coords="7.0 2.0"/>
      <Node id="5" coords="10.0 0.0"/>
      <Node coords="10.0 2.0"/>
    </Nodes>
    <Elements>
      <L4Axisymm nodes="1 3 4 2" boundaryloads="3 4"/>
      <L4Axisymm nodes="3 5 6 4" bodyloads="4"/>
    </Elements>
    <CrossSections>
      <SimpleCS material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="1000.0" n="0.0" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="1"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="0.0 0.0" set="2"/>
      <ConstantEdgeLoad loadtimefunction="1" dofs="1 2" components="10.0 0.0" loadtype="3"/>
      <DeadWeight loadtimefunction="1" components="0.0 0.0"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1 2"/>
      <Set elementboundaries="1 4"/>
      <Set elements="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-06">
      <!-- Exact solution is 5.523809523809524e-2 -->
      <NODE tStep="1" number="1" dof="1" unknown="d" value="0.0552380952"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0552380952"/>
      <!-- Exact solution is 4.054421768707483e-2 -->
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.0405442177"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.0405442177"/>
      <!-- Exact solution is 3.809523809523809e-2 -->
      <NODE tStep="1" number="5" dof="1" unknown="d" value="0.0380952381"/>
      <NODE tStep="1" number="6" dof="1" unknown="d" value="0.0380952381"/>
      <!-- Constant strains and stresses over an element -->
      <!-- Element 1 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="-0.004898"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="3" value="0.0087075"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="4" component="1" value="-0.004898"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="4" component="3" value="0.0087075"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="4" component="6" value="0.0"/>
      <!-- Exact solution at r=4.633 sig_r=-6.965, sig_theta=10.77 -->
      <!-- Exact solution at r=6.366 sig_r=-2.80, sig_theta=6.60 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-4.89795918"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="3" value="8.70748299"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="1" component="1" value="-4.89795918"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="1" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="1" component="3" value="8.70748299"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="1" component="6" value="0.0"/>
      <!-- Element 2 -->
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="1" value="-0.00081633"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="3" value="0.0046259"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="4" component="1" value="-0.00081633"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="4" component="3" value="0.0046259"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="4" component="6" value="0.0"/>
      <!-- Exact solution at r=7.633 sig_r=-1.36, sig_theta=5.17 -->
      <!-- Exact solution at r=9.366 sig_r=-0.27, sig_theta=4.08 -->
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="1" value="-0.816326531"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="3" value="4.62585034"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="1" value="-0.816326531"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="3" value="4.62585034"/>
      <ELEMENT tStep="1" number="2" gp="4" keyword="1" component="6" value="0.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
