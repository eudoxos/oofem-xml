<oofem version="1">
  <Output>patch107.out</Output>
  <Description>Patch test of trplanestrain elements</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0"/>
      <Node coords="6.0 0.0"/>
      <Node coords="6.0 3.0"/>
      <Node coords="0.0 3.0"/>
      <Node id="5" coords="2.0 1.0"/>
    </Nodes>
    <Elements>
      <TrPlaneStrain nodes="1 2 5"/>
      <TrPlaneStrain nodes="2 3 5"/>
      <TrPlaneStrain nodes="3 4 5"/>
      <TrPlaneStrain nodes="4 1 5"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="10.0" n="0.2" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="1.5 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="1 4"/>
      <Set nodes="2 3"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-12">
      <!--Note: this is the exact solution-->
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="-1.5"/>
      <REACTION tStep="1" number="1" dof="2" value="-0.75"/>
      <REACTION tStep="1" number="2" dof="2" value="-0.75"/>
      <REACTION tStep="1" number="3" dof="2" value="0.75"/>
      <REACTION tStep="1" number="4" dof="1" value="-1.5"/>
      <REACTION tStep="1" number="4" dof="1" value="-1.5"/>
      <REACTION tStep="1" number="4" dof="2" value="0.75"/>
      <!--check all nodes-->
      <NODE tStep="1" number="1" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="1" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.54"/>
      <NODE tStep="1" number="2" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.54"/>
      <NODE tStep="1" number="3" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="0.0"/>
      <NODE tStep="1" number="5" dof="1" unknown="d" value="0.18"/>
      <NODE tStep="1" number="5" dof="2" unknown="d" value="0.0"/>
      <!--check element no. 1 strain vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.09"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="0.0"/>
      <!--check element no. 1 stress vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="1.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="0.25"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="1" value="0.09"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="1" value="1.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="2" value="0.25"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="1" value="0.09"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="1" value="1.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="2" value="0.25"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="1" value="0.09"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="1" value="1.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="2" value="0.25"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="6" value="0.0"/>
    </errorcheck>
