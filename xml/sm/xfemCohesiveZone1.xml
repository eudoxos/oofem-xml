<oofem version="1">
  <Output>xfemCohesiveZone1.out</Output>
  <Description>XFEM simulation: A single element with bilinear cohesive zone, loaded in pure shear. Written by Erik Svenning, Chalmers University of Technology.</Description>
  <Analysis type="StaticStructural" nsteps="5" deltat="1.0" rtolf="1e-06" maxiter="25" miniter="2" manrmsteps="1"/>
  <!-- vtkxml tstep_all domain_all primvars 1 1 cellvars 1 1 -->
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0"/>
      <Node coords="1.0 0.0"/>
      <Node coords="1.0 1.0"/>
      <Node coords="0.0 1.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2DXfem nodes="1 2 3 4" nip="9" nlgeo="0" czmaterial="2"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <!-- Linear elasticity -->
      <IsoLE d="0.0" e="100000.0" n="0.0" talpha="0.0"/>
      <!-- Bilinear cohesive zone material -->
      <intmatbilinearcz kn="1000000.0" g1c="10.0" g2c="1.5" mu="0.0" gamma="0.5" sigf="100.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.08 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <PiecewiseLinFunction t="1.0 6.0" f_t_="0.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1 2"/>
      <Set nodes="3 4"/>
    </Sets>
    <XFemManager type="XfemManager" debugvtk="0">
      <EnrichmentItems>
        <crack>
          <!-- DiscontinuousFunction 1 -->
          <EnrichmentFunction type="HeavisideFunction"/>
          <!-- polygoncrack 1 points 6 -0.1 0.3 0.5 0.3 1.1 0.3 -->
          <EnrichmentGeometry type="PolygonLine" points="-0.5 0.3 -0.2 0.3 1.1 0.3"/>
        </crack>
      </EnrichmentItems>
    </XFemManager>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!-- step 2 -->
      <REACTION tStep="2" number="3" dof="1" value="18.7798081"/>
      <REACTION tStep="2" number="4" dof="1" value="18.7798262"/>
      <!-- step 3 -->
      <REACTION tStep="3" number="3" dof="1" value="11.9649016"/>
      <REACTION tStep="3" number="4" dof="1" value="11.9649061"/>
      <!-- step 4 -->
      <REACTION tStep="4" number="3" dof="1" value="5.14994041"/>
      <REACTION tStep="4" number="4" dof="1" value="5.1499449"/>
      <!-- step 5 -->
      <REACTION tStep="5" number="3" dof="1" value="0.0"/>
      <REACTION tStep="5" number="4" dof="1" value="0.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
