<oofem version="1">
  <Output>interface01.out</Output>
  <Description>2D quadrilateral with one contacts. The contact works in compression and separates in tension.</Description>
  <Analysis type="StaticStructural" nsteps="10" rtolf="0.001" manrmsteps="30" deltat="1.0"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.5 0.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
      <Node coords="0.5 1.0 0.0"/>
      <Node id="5" coords="0.5 1.0 0.0"/>
    </Nodes>
    <Elements>
      <PlaneStress2D nodes="1 2 4 3" crosssect="1"/>
      <Interface1d nodes="4 5" normal="1.0 0.0"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.1" material="1"/>
      <InterfaceCS thickness="0.1" material="2" set="4"/>
    </CrossSections>
    <Materials>
      <IsoLe d="2500.0" e="40000.0" n="0.15" talpha="2.4e-06"/>
      <simpleintermat kn="1000.0" fc="1.0" stiffcoeff="0.2" regularized="1"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="1"/>
      <BoundaryCondition loadtimefunction="2" dofs="1" values="0.01" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinFunction t="1.0 5.0 10.0" f_t_="0.0 -1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="1"/>
      <Set nodes="5"/>
      <Set noderanges="1-5"/>
      <Set elements="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!-- Step 1 -->
      <NODE tStep="2" number="4" dof="1" unknown="d" value="-0.00139973869"/>
      <ELEMENT tStep="2" number="2" gp="1" keyword="99" component="1" value="-1.1003"/>
      <REACTION tStep="2" number="5" dof="1" value="-1.1003"/>
      <!-- Step 10 -->
      <NODE tStep="10" number="4" dof="1" unknown="d" value="0.00202829954"/>
      <ELEMENT tStep="10" number="2" gp="1" keyword="99" component="1" value="1.5943"/>
      <REACTION tStep="10" number="5" dof="1" value="1.5943"/>
    </errorcheck>
  </ExportModules>
</oofem>
