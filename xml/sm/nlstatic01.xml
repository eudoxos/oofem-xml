<oofem version="1">
  <Output>nlstatic01.out</Output>
  <Description>Test of LIbeam2d elements -&gt; simply supported beam</Description>
  <Analysis type="StaticStructural" nsteps="1" solvertype="calm" steplength="100.0" minsteplength="100.0" rtolf="0.0001" psi="1.0"/>
  <Domain domain="2dbeam">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="5.0 0.0 0.0"/>
      <Node coords="10.0 0.0 0.0"/>
    </Nodes>
    <Elements>
      <Beam2d nodes="1 2" nip="4"/>
      <Beam2d nodes="2 3" nip="4"/>
    </Elements>
    <CrossSections>
      <SimpleCS area="0.3" iy="0.00225" beamshearcoeff="1e+18" thick="0.3" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <ISoLE e="30000000000.0" n="0.25" talpha="1.2e-05" d="0.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.0" set="3"/>
      <ConstantEdgeLoad loadtimefunction="1" components="0.0 100.0 0.0" loadtype="3" set="4" reference=""/>
      <ConstantEdgeLoad loadtimefunction="1" components="0.0 100.0 0.0" loadtype="3" set="5" reference=""/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="1"/>
      <Set nodes="3"/>
      <Set elementedges="1 1"/>
      <Set id="5" elementedges="2 1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-06">
      <LOADLEVEL tStep="1" value="0.17231"/>
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="3" value="-86.154979"/>
      <REACTION tStep="1" number="3" dof="3" value="-86.154979"/>
      <NODE tStep="1" number="2" dof="3" unknown="d" value="3.32388036e-05"/>
    </errorcheck>
  </ExportModules>
</oofem>
