<oofem version="1">
  <Output>idm02.out</Output>
  <Description>Test of damage law with exponential softening on a 1D truss element, crack opening</Description>
  <Analysis type="StaticStructural" nsteps="15" solvertype="calm" rtolf="0.0001" maxiter="20" psi="0.0" hpcmode="1" hpc="2 1" steplength="0.05" minsteplength="0.05"/>
  <!-- vtkxml tstep_all domain_all primvars 1 1 -->
  <Domain domain="1dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.5 0.0 0.0"/>
    </Nodes>
    <Elements>
      <truss1d nodes="1 2" mat="1"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" width="10.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <!-- exponential softening, fracturing strain -->
      <idm1 d="1.0" e="10.0" n="0.2" e0="0.5" wf="0.6" equivstraintype="0" talpha="0.0" damlaw="0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <NodalLoad loadtimefunction="1" dofs="1" components="1.0" set="3" reference=""/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- Used for Extractor -->
    <errorcheck tolerance="0.0001">
      <NODE tStep="11" number="2" dof="1" unknown="d" value="0.55"/>
      <LOADLEVEL tStep="11" value="24.52618"/>
      <LOADLEVEL tStep="12" value="22.11659"/>
      <LOADLEVEL tStep="13" value="19.99098"/>
      <LOADLEVEL tStep="14" value="18.10592"/>
      <LOADLEVEL tStep="15" value="16.42678"/>
    </errorcheck>
  </ExportModules>
</oofem>
