<oofem version="1">
  <Output>timestepreduction.out</Output>
  <Description>test of time step reduction in a case of Neo-Hookean Material usign Ogden material with alpha = 2, mu = 20, and K = 10.</Description>
  <Analysis type="StaticStructural" finalt="0.1" treductiontype="SimpleReduction" deltat="1.0" rtolf="1e-08" maxiter="6" dtmax="2.0" dtmin="0.01" stiffmode="0" manrmsteps="1" ncrf="0.2"/>
  <Domain domain="3d">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <!-- &#8212;-Nodes&#8212;-############################### -->
      <Node coords="0.0 0.0"/>
      <Node coords="1.0 0.0"/>
      <Node coords="1.0 1.0"/>
      <Node coords="0.0 1.0"/>
    </Nodes>
    <Elements>
      <!-- &#8212;-Elements&#8212;-############################# -->
      <quad1PlaneStrain nodes="1 2 3 4" mat="1" crosssect="1" nlgeo="1"/>
    </Elements>
    <CrossSections>
      <!-- &#8212;-Cross-Sections&#8212;-########################### -->
      <SimpleCS thick="1.0" set="1" material="1"/>
    </CrossSections>
    <Materials>
      <!-- &#8212;-Materials&#8212;-############################# -->
      <OgdenCompressibleMat d="1.0" alpha="2.0" mu="20.0" k="10.0"/>
    </Materials>
    <BoundaryConditions>
      <!-- &#8212;-Boundary cond.&#8212;-######################## -->
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="2" dofs="2" values="10.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <!-- &#8212;-Time functions&#8212;-######################## -->
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinFunction npoints="3" t="-1.0 0.0 2000.0" f_t_="0.0 1.0 2001.0"/>
    </LoadTimeFunctions>
    <Sets>
      <!-- &#8212;-Sets&#8212;-############################## -->
      <Set elementranges="1-1"/>
      <Set nodes="1 2"/>
      <Set nodes="3 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.001">
      <!-- check reactions -->
      <REACTION tStep="1" number="1" dof="1" value="9.4444"/>
      <REACTION tStep="1" number="1" dof="2" value="-7.3924"/>
      <REACTION tStep="1" number="2" dof="1" value="-9.4444"/>
      <REACTION tStep="1" number="2" dof="2" value="-7.3924"/>
      <REACTION tStep="1" number="3" dof="2" value="7.3924"/>
      <REACTION tStep="1" number="4" dof="2" value="7.3924"/>
      <!-- check all nodes -->
      <NODE tStep="1" number="3" dof="1" unknown="d" value="3.45508878"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="-3.45508878"/>
      <!-- check element deformation gradient -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="91" component="1" value="6.4499"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="91" component="2" value="11.4"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="91" component="6" value="1.9948"/>
      <!-- check element first Piola-Kirchhoff stress -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="92" component="1" value="3.6163"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="92" component="2" value="10.878"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="92" component="3" value="-22.941"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="92" component="6" value="2.2731"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="92" component="9" value="0.6533"/>
    </errorcheck>
  </ExportModules>
</oofem>
