<oofem version="1">
  <Output>EC2creep_casting.out</Output>
  <Description>Test on material casting. Before casting time the material is characterized by properties of "preCastingTimeMaterial"</Description>
  <Analysis type="StaticStructural" nsteps="40" prescribedtimes="0.0001 0.0002 0.0005 0.001 0.002 0.005 0.01 0.02 0.05 0.1 0.2 0.5 1.0 2.0 5.0 10.0 10.0001 10.0002 10.0005 10.001 10.002 10.005 10.01 10.02 10.05 10.1 10.2 10.5 11.0 12.0 15.0 20.0 50.0 100.0 200.0 500.0 1000.0 2000.0 5000.0 10000.0" miniter="1" maxiter="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <!--NODES-->
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.1 0.0 0.0"/>
      <Node coords="0.0 0.1 0.0"/>
      <Node coords="0.1 0.1 0.0"/>
    </Nodes>
    <Elements>
      <!--ELEMENTS-->
      <planestress2d nodes="1 2 4 3" crosssect="1"/>
    </Elements>
    <CrossSections>
      <!--CROSSECTION-->
      <SimpleCS thick="1.0" width="1.0" material="1"/>
    </CrossSections>
    <Materials>
      <!--MATERIAL-->
      <EC2CreepMat d="0.0" n="0.2" fcm28="30.0" stiffnessfactor="1000000.0" relmatage="7.0" t0="7.0" timefactor="1.0" cemtype="2" henv="0.5" h0="100.0" shtype="0" begoftimeofinterest="1.0" endoftimeofinterest="10.0" spectrum="" castingtime="10.0" precastingtimemat="2"/>
      <IsoLE d="0.0" n="0.2" e="1e-06" talpha="0.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="1"/>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="5e-11 0.0" set="3"/>
      <NodalLoad loadtimefunction="2" dofs="1 2" components="0.05 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <!--TIME FUNCTION-->
      <ConstantFunction f_t_="1.0"/>
      <HeavisideLTF origin="10.0" value="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="1"/>
      <Set nodes="3"/>
      <Set nodes="2 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!--_#%BEGIN_CHECK%-->
    <!--TIME-->
    <!--NODE  number 4 dof 1 unknown d-->
    <!--_#%END_CHECK%-->
    <errorcheck tolerance="1e-09">
      <NODE tStep="17" number="4" dof="1" unknown="d" value="0.0001047935"/>
      <NODE tStep="25" number="4" dof="1" unknown="d" value="0.0001048435"/>
      <NODE tStep="40" number="4" dof="1" unknown="d" value="0.0001131202"/>
    </errorcheck>
  </ExportModules>
</oofem>
