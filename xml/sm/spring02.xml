<oofem version="1">
  <Output>spring02.out</Output>
  <Description>Patch test of spring element in xy plane</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 0.0 0.0"/>
    </Nodes>
    <Elements>
      <Spring nodes="1 2" mode="1" orientation="1.0 0.0 0.0" k="2.0"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.1" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <dummymat/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="1.0 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!-- check nodal displacements -->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.5"/>
      <!-- element stresses and strains -->
      <!-- ELEMENT tStep 1 number 1 keyword "moment" component 1  value 1.0 -->
      <REACTION tStep="1" number="1" dof="1" value="-1.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
