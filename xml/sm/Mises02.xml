<oofem version="1">
  <Output>Mises02.out</Output>
  <Description>Test of Mises plasticity model with multilinear hardening/softening</Description>
  <Analysis type="StaticStructural" nsteps="10" rtolf="0.0001" maxiter="20"/>
  <Domain domain="1dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.5 0.0 0.0"/>
    </Nodes>
    <Elements>
      <truss1d nodes="1 2"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" width="10.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <MisesMat d="1.0" talpha="0.0" e="1.0" n="0.2" htype="1" h_eps="0.0 3.0 25.0" h_eps_="1.0 2.0 1.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="2" dofs="1" values="1.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <PiecewiseLinFunction t="0.0 11.0" f_t_="0.0 12.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!--Used for Extractor-->
    <errorcheck tolerance="0.0001">
      <ELEMENT tStep="3" number="1" gp="1" keyword="4" component="1" value="6.5455"/>
      <ELEMENT tStep="3" number="1" gp="1" keyword="1" component="1" value="1.9264"/>
      <ELEMENT tStep="20" number="1" gp="1" keyword="4" component="1" value="21.818"/>
      <ELEMENT tStep="20" number="1" gp="1" keyword="1" component="1" value="1.1991"/>
    </errorcheck>
  </ExportModules>
</oofem>
