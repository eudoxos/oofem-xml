<oofem version="1">
  <Output>InterfaceEL_Point3D_03.out</Output>
  <Description>Test of cohesive interface between two points, loaded in x by tension/compression/shear.</Description>
  <Analysis type="StaticStructural" nsteps="1" rtolf="0.0001" manrmsteps="5" deltat="1.0"/>
  <Domain domain="3d">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="0.0 2.0 0.0"/>
      <Node coords="0.0 0.0 2.0"/>
      <Node id="5" coords="-2.0 0.0 0.0"/>
      <Node coords="0.0 -2.0 0.0"/>
      <Node coords="0.0 0.0 -2.0"/>
    </Nodes>
    <Elements>
      <IntELPoint nodes="1 2" normal="1.0 0.0 0.0"/>
      <IntELPoint nodes="1 3" normal="0.0 1.0 0.0"/>
      <IntELPoint nodes="1 4" normal="0.0 0.0 1.0"/>
      <IntELPoint nodes="1 5" normal="-1.0 0.0 0.0"/>
      <IntELPoint id="5" nodes="1 6" normal="0.0 -1.0 0.0"/>
      <IntELPoint nodes="1 7" normal="0.0 0.0 -1.0"/>
    </Elements>
    <CrossSections>
      <InterfaceCS material="1" set="3"/>
    </CrossSections>
    <Materials>
      <CohInt kn="20000.0" ks="13000.0" stiffcoeffkn="0.03" transitionopening="0.0" smoothmag="100000000.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2 3" values="0.0 0.0 0.0" set="1"/>
      <nodalload loadtimefunction="1" dofs="1 2 3" components="6.0 0.0 0.0" set="2"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="1"/>
      <Set nodes="2 3 4 5 6 7"/>
      <Set elementranges="1-6"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-05">
      <!--Step 1-->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.01"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.000461538462"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.000461538462"/>
      <NODE tStep="1" number="5" dof="1" unknown="d" value="0.0003"/>
      <NODE tStep="1" number="6" dof="1" unknown="d" value="0.000461538462"/>
      <NODE tStep="1" number="7" dof="1" unknown="d" value="0.000461538462"/>
    </errorcheck>
  </ExportModules>
</oofem>
