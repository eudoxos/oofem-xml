<oofem version="1">
  <Output>patch010.out</Output>
  <Description>Patch test of Truss2d elements -&gt; temperature load</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dtruss">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="2.0 0.0 0.0"/>
      <Node coords="4.0 0.0 0.0"/>
      <Node coords="6.0 0.0 0.0"/>
      <Node id="5" coords="8.0 0.0 0.0"/>
    </Nodes>
    <Elements>
      <Truss2d nodes="1 2"/>
      <Truss2d nodes="2 3"/>
      <Truss2d nodes="3 4"/>
      <Truss2d nodes="4 5"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.1" width="10.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE talpha="1.2e-05" d="1.0" e="1.0" n="0.2"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 3" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.0" set="3"/>
      <StructTemperatureLoad loadtimefunction="1" components="30.0 0.0" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="1 5"/>
      <Set nodes="2 3 4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-08">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="0.00036"/>
      <REACTION tStep="1" number="1" dof="3" value="0.0"/>
      <REACTION tStep="1" number="2" dof="3" value="0.0"/>
      <REACTION tStep="1" number="3" dof="3" value="0.0"/>
      <REACTION tStep="1" number="4" dof="3" value="0.0"/>
      <REACTION tStep="1" number="5" dof="1" value="-0.00036"/>
      <REACTION tStep="1" number="5" dof="3" value="0.0"/>
      <!--check all nodes-->
      <NODE tStep="1" number="1" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="5" dof="1" unknown="d" value="0.0"/>
      <NODE tStep="1" number="1" dof="3" unknown="d" value="0.0"/>
      <NODE tStep="1" number="2" dof="3" unknown="d" value="0.0"/>
      <NODE tStep="1" number="3" dof="3" unknown="d" value="0.0"/>
      <NODE tStep="1" number="4" dof="3" unknown="d" value="0.0"/>
      <NODE tStep="1" number="5" dof="3" unknown="d" value="0.0"/>
      <!--check element no. 1 strain vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="4" component="1" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="1" value="0.0"/>
      <!--check element no. 1 stress vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-0.00036"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="1" value="-0.00036"/>
      <ELEMENT tStep="1" number="3" gp="1" keyword="1" component="1" value="-0.00036"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="1" value="-0.00036"/>
    </errorcheck>
  </ExportModules>
</oofem>
