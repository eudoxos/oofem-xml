<oofem version="1">
  <Output>hangingnode01.out</Output>
  <Description>Test of hanging node in PlaneStress2d element</Description>
  <Analysis type="LinearStatic" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <Node coords="8.0 4.0 0.0"/>
      <Node coords="8.0 0.0 0.0"/>
      <hangingnode id="5" coords="6.0 1.8 0.0" masterelement="1" doftype="2 2"/>
    </Nodes>
    <Elements>
      <PlaneStress2d nodes="1 4 3 2"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.15" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="0.0" e="15.0" n="0.25" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="2.5 0.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1"/>
      <Set nodes="2"/>
      <Set nodes="5"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.0001">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="-1.375"/>
      <REACTION tStep="1" number="2" dof="1" value="-1.125"/>
      <!--check nodes-->
      <NODE tStep="1" number="2" dof="2" unknown="d" value="-0.208333333"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="1.19791667"/>
      <NODE tStep="1" number="3" dof="2" unknown="d" value="0.729166667"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="2.13541667"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="0.9375"/>
      <NODE tStep="1" number="5" dof="1" unknown="d" value="1.28515625"/>
      <NODE tStep="1" number="5" dof="2" unknown="d" value="0.609375"/>
      <!--check element no. 1 strain vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.1745"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="-0.052083"/>
      <!--check element no. 1 stress vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="2.5837"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="-0.13532"/>
    </errorcheck>
  </ExportModules>
</oofem>
