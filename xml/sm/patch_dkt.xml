<oofem version="1">
  <Output>patch_dkt.out</Output>
  <Description>"Complex Patch test of DKT elements, four cases pure bending in x, and y, twist, and shear in one"</Description>
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dmindlinplate">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <!-- pure bending along x axis -->
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="4.0 0.0 0.0"/>
      <Node coords="4.0 4.0 0.0"/>
      <Node coords="0.0 4.0 0.0"/>
      <!-- pure bending along y axis -->
      <Node id="11" coords="5.0 0.0 0.0"/>
      <Node coords="9.0 0.0 0.0"/>
      <Node coords="9.0 4.0 0.0"/>
      <Node coords="5.0 4.0 0.0"/>
      <!-- pure twist -->
      <Node id="21" coords="10.0 0.0 0.0"/>
      <Node coords="14.0 0.0 0.0"/>
      <Node coords="14.0 4.0 0.0"/>
      <Node coords="10.0 4.0 0.0"/>
      <!-- shear x -->
      <Node id="31" coords="15.0 0.0 0.0"/>
      <Node coords="19.0 0.0 0.0"/>
      <Node coords="19.0 4.0 0.0"/>
      <Node coords="15.0 4.0 0.0"/>
      <!-- shear y -->
      <Node id="41" coords="20.0 0.0 0.0"/>
      <Node coords="24.0 0.0 0.0"/>
      <Node coords="24.0 4.0 0.0"/>
      <Node coords="20.0 4.0 0.0"/>
    </Nodes>
    <Elements>
      <dktplate nodes="1 2 3"/>
      <dktplate nodes="1 3 4"/>
      <dktplate id="11" nodes="11 12 13"/>
      <dktplate nodes="11 13 14"/>
      <dktplate id="21" nodes="21 22 23"/>
      <dktplate nodes="21 23 24"/>
      <dktplate id="31" nodes="31 32 33"/>
      <dktplate nodes="31 33 34"/>
      <dktplate id="41" nodes="41 42 43"/>
      <dktplate nodes="41 43 44"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.3" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="2500.0" e="15.0" n="0.25" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="3" values="0.0" set="5"/>
      <BoundaryCondition loadtimefunction="1" dofs="4" values="0.0" set="6"/>
      <BoundaryCondition loadtimefunction="1" dofs="5" values="0.0" set="7"/>
      <NodalLoad loadtimefunction="1" dofs="3 4 5" components="0.0 -2.5 0.0" set="2"/>
      <NodalLoad id="5" loadtimefunction="1" dofs="3 4 5" components="0.0 0.0 -2.5" set="3"/>
      <NodalLoad loadtimefunction="1" dofs="3 4 5" components="0.5 0.0 0.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1,2,11,12,21,22,31,32,41,42"/>
      <Set nodes="3 4"/>
      <Set nodes="12 13"/>
      <Set nodes="23 32 33 43 44"/>
      <Set id="5" nodes="1 2 11 14 21 22 24 31 34 41 42"/>
      <Set nodes="1 2 11 12 13 14 31 32 33 34 41 42"/>
      <Set nodes="1 2 3 4 11 14 31 34 41 42 43 44"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- exact solution -->
    <errorcheck tolerance="0.0005">
      <!-- check reactions -->
      <!-- pure bending along x axis -->
      <REACTION tStep="1" number="1" dof="3" value="0.0"/>
      <REACTION tStep="1" number="1" dof="4" value="2.5"/>
      <REACTION tStep="1" number="2" dof="3" value="0.0"/>
      <REACTION tStep="1" number="2" dof="4" value="2.5"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="4" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="5" value="34.722"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="2" keyword="4" component="4" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="2" keyword="4" component="5" value="34.722"/>
      <ELEMENT tStep="1" number="1" gp="2" keyword="4" component="6" value="0.0"/>
