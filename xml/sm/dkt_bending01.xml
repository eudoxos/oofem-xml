<oofem version="1">
  <Output>dkt_bending01.out</Output>
  <Description>"Patch test of DKT elements -&gt; pure bending of a square clamped plate"</Description>
  <Analysis type="LinearStatic" nsteps="2"/>
  <Domain domain="2dmindlinplate">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="8.0 0.0 0.0"/>
      <Node coords="8.0 8.0 0.0"/>
      <Node coords="0.0 8.0 0.0"/>
    </Nodes>
    <Elements>
      <DKTPlate nodes="1 2 3"/>
      <DKTPlate nodes="3 4 1" boundaryloads="3 1"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="1.0" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="2500.0" e="10000.0" n="0.3" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="3 4" values="0.0 0.0" set="3"/>
      <nodalLoad loadtimefunction="2" dofs="3 4 5" components="0.0 12.0 0.0" set="2"/>
      <ConstantEdgeLoad loadtimefunction="3" dofs="3 4 5" components="0.0 3.0 0.0" set="0"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
      <PeakFunction t="1.0" f_t_="1.0"/>
      <PeakFunction t="2.0" f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-2"/>
      <Set nodes="3 4"/>
      <Set nodes="1 2"/>
      <!-- TODO: Should we have an edge or surface set here? -->
      <Set elementboundaries="2 1"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- exact solution -->
    <!-- DISPLACEMENT             w -->
    <!-- node 3                   0.2496 -->
    <!-- STRAIN                     STRESS -->
    <!-- kapa_x  =  1.0800e-03             m_x  =  0.0 -->
    <!-- kapa_y  = -3.6000e-03             m_y  = -3.0 -->
    <!-- kapa_xy =  0.0                    m_xy =  0.0 -->
    <!-- gama_xz =  0.0                    q_x  =  0.0 -->
    <!-- gama_yz =  0.0                    q_y  =  0.0 -->
    <errorcheck tolerance="0.0001">
      <NODE tStep="1" number="1" dof="5" unknown="d" value="-0.00432"/>
      <NODE tStep="1" number="2" dof="5" unknown="d" value="0.00432"/>
      <NODE tStep="1" number="3" dof="3" unknown="d" value="0.1152"/>
      <NODE tStep="1" number="3" dof="4" unknown="d" value="0.0288"/>
      <NODE tStep="1" number="3" dof="5" unknown="d" value="0.00432"/>
      <NODE tStep="1" number="4" dof="3" unknown="d" value="0.1152"/>
      <NODE tStep="1" number="4" dof="4" unknown="d" value="0.0288"/>
      <NODE tStep="1" number="4" dof="5" unknown="d" value="-0.00432"/>
      <!-- check element no. 1 stress vector -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="4" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="5" value="-3.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="6" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="4" value="0.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="5" value="-3.0"/>
      <ELEMENT tStep="1" number="2" gp="1" keyword="1" component="6" value="0.0"/>
    </errorcheck>
  </ExportModules>
</oofem>
