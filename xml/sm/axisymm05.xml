<oofem version="1">
  <Output>axisymm05.out</Output>
  <Description>Test of linear axisymmetric triangular elements, thick ring with internal overpressure, test based on lectures Advanced Finite Elements for Solids, Plates and Shells from Felippa, Colorado, Chapter 6, version 2013, pp. 6-11. Exact solution is on page 7-4.</Description>
  <!-- Exact solution for radial displacement u_r = (100+r^2)/(525*r) -->
  <!-- Exact solution for radial stress sig_r = 40./21.*(1-100./r^2) -->
  <!-- Exact solution for hoop stress sig_theta = 40./21.*(1+100./r^2) -->
  <!-- LinearStatic nsteps 1 nmodules 1 -->
  <Analysis type="StaticStructural" nsteps="1"/>
  <!-- vtkxml tstep_step 1 cellvars 1 46 vars 3 1 2 4 primvars 1 1 stype 2 -->
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="4.0 0.0"/>
      <Node coords="4.0 2.0"/>
      <Node coords="7.0 0.0"/>
      <Node coords="7.0 2.0"/>
      <Node id="5" coords="10.0 0.0"/>
      <Node coords="10.0 2.0"/>
    </Nodes>
    <Elements>
      <Axisymm3d nodes="1 4 2" boundaryloads="3 3"/>
      <Axisymm3d nodes="1 3 4"/>
      <Axisymm3d nodes="3 6 4"/>
      <Axisymm3d nodes="3 5 6"/>
    </Elements>
    <CrossSections>
      <SimpleCS material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="1000.0" n="0.0" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="1"/>
      <NodalLoad loadtimefunction="1" dofs="1 2" components="0.0 0.0" set="2"/>
      <ConstantEdgeLoad loadtimefunction="1" dofs="1 2" components="10.0 0.0" loadtype="3"/>
      <DeadWeight loadtimefunction="1" components="0.0 0.0" set="4"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1-4"/>
      <Set nodes="1 2"/>
      <Set elementboundaries="1 3"/>
      <Set elements="4"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-06">
      <!-- Exact solution is 5.523809523809524e-2 -->
      <NODE tStep="1" number="1" dof="1" unknown="d" value="0.0537490912"/>
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.0557736672"/>
      <!-- Exact solution is 4.054421768707483e-2 -->
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.0405682312"/>
      <NODE tStep="1" number="4" dof="1" unknown="d" value="0.0400335683"/>
      <!-- Exact solution is 3.809523809523809e-2 -->
      <NODE tStep="1" number="5" dof="1" unknown="d" value="0.0382820662"/>
      <NODE tStep="1" number="6" dof="1" unknown="d" value="0.0375270371"/>
      <!-- Constant strains and stresses over an element -->
      <!-- Element 1 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="-0.0052467"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="3" value="0.0099704"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="6" value="0.0010123"/>
      <!-- Exact solution at r=5.0 sig_r=-5.71, sig_theta=9.52 -->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="-5.2467"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="3" value="9.97042178"/>
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="6" value="0.506144011"/>
      <!-- Element 4 -->
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="1" value="-0.00076205"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="3" value="0.0043103"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="4" component="6" value="-0.00037751"/>
      <!-- Exact solution at r=9.0 sig_r=-0.45, sig_theta=4.26 -->
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="1" value="-0.762054992"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="2" value="0.0"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="3" value="4.31027165"/>
      <ELEMENT tStep="1" number="4" gp="1" keyword="1" component="6" value="-0.18875728"/>
    </errorcheck>
  </ExportModules>
</oofem>
