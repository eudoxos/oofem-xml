<oofem version="1">
  <Output>deadweight01.out</Output>
  <Description>Test of deadweight on quadratic QPlaneStress2d element</Description>
  <!--The analytical solution (x is the coordinate from the right edge oriented leftwards) reads:-->
  <!--F(x) = 2x-->
  <!--sigma(x)=10x-->
  <!--epsilon(x)=0.1x-->
  <!--displacement_x(x)=0.05x^2-0.8-->
  <Analysis type="StaticStructural" nsteps="1"/>
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="4.0 0.0 0.0"/>
      <Node coords="4.0 1.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
      <Node id="5" coords="2.0 0.0 0.0"/>
      <Node coords="4.0 0.5 0.0"/>
      <Node coords="2.0 1.0 0.0"/>
      <Node coords="0.0 0.5 0.0"/>
    </Nodes>
    <Elements>
      <QPlaneStress2d nodes="1 2 3 4 5 6 7 8" nip="9"/>
    </Elements>
    <CrossSections>
      <SimpleCS thick="0.2" material="1" set="1"/>
    </CrossSections>
    <Materials>
      <IsoLE d="1.0" e="100.0" n="0.01" talpha="1.2e-05"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <BoundaryCondition loadtimefunction="1" dofs="2" values="0.0" set="3"/>
      <DeadWeight loadtimefunction="1" components="10.0 0.0" set="1"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set elementranges="1"/>
      <Set nodes="1 4 8"/>
      <Set nodes="6 8"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="0.001">
      <!--check reactions-->
      <REACTION tStep="1" number="1" dof="1" value="-1.3303"/>
      <REACTION tStep="1" number="4" dof="1" value="-1.3303"/>
      <REACTION tStep="1" number="8" dof="1" value="-5.3393"/>
      <!--check displacements in right nodes-->
      <NODE tStep="1" number="2" dof="1" unknown="d" value="0.799872412"/>
      <NODE tStep="1" number="3" dof="1" unknown="d" value="0.799872412"/>
      <NODE tStep="1" number="4" dof="2" unknown="d" value="-0.00195877842"/>
      <NODE tStep="1" number="6" dof="1" unknown="d" value="0.800063794"/>
      <!--check element No. 1 strain vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="4" component="1" value="0.045068"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="4" component="1" value="0.19999"/>
      <ELEMENT tStep="1" number="1" gp="7" keyword="4" component="1" value="0.35491"/>
      <!--check element No. 1 stress vector-->
      <ELEMENT tStep="1" number="1" gp="1" keyword="1" component="1" value="4.5068"/>
      <ELEMENT tStep="1" number="1" gp="4" keyword="1" component="1" value="19.999"/>
      <ELEMENT tStep="1" number="1" gp="7" keyword="1" component="1" value="35.491"/>
    </errorcheck>
  </ExportModules>
</oofem>
