<oofem version="1">
  <Output>EC2creep.out</Output>
  <Description>creep in tension with two material variants: least squares method and retardation spectrum</Description>
  <Analysis type="StaticStructural" nsteps="25" prescribedtimes="0.0001 0.0002 0.0005 0.001 0.002 0.005 0.01 0.02 0.05 0.1 0.2 0.5 1.0 2.0 5.0 10.0 20.0 50.0 100.0 200.0 500.0 1000.0 2000.0 5000.0 10000.0"/>
  <!-- vtkxml tstep_step 1 domain_all vars 2 1 4 primvars 1 1 -->
  <Domain domain="2dplanestress">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <!-- NODES -->
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="0.1 0.0 0.0"/>
      <Node coords="0.0 0.1 0.0"/>
      <Node coords="0.1 0.1 0.0"/>
      <Node id="5" coords="0.0 0.2 0.0"/>
      <Node coords="0.1 0.2 0.0"/>
      <Node coords="0.0 0.3 0.0"/>
      <Node coords="0.1 0.3 0.0"/>
    </Nodes>
    <Elements>
      <!-- ELEMENTS -->
      <planestress2d nodes="1 2 4 3" crosssect="1"/>
      <planestress2d nodes="5 6 8 7" crosssect="2"/>
    </Elements>
    <Sets>
      <Set nodes="1 5"/>
      <Set nodes="3 7"/>
      <Set nodes="2 4 6 8"/>
    </Sets>
    <CrossSections>
      <!-- CROSSECTION -->
      <SimpleCS thick="1.0" width="1.0" material="1"/>
      <SimpleCS thick="1.0" width="1.0" material="2"/>
    </CrossSections>
    <Materials>
      <!-- MATERIAL -->
      <EC2CreepMat d="0.0" n="0.2" fcm28="30.0" stiffnessfactor="1000000.0" relmatage="7.0" t0="7.0" timefactor="1.0" cemtype="2" henv="0.5" h0="100.0" shtype="0" begoftimeofinterest="0.1" endoftimeofinterest="1000.0" spectrum=""/>
      <EC2CreepMat d="0.0" n="0.2" fcm28="30.0" stiffnessfactor="1000000.0" relmatage="7.0" t0="7.0" timefactor="1.0" cemtype="2" henv="0.5" h0="100.0" shtype="0" begoftimeofinterest="10.0" endoftimeofinterest="1000.0"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" dofs="1 2" values="0.0 0.0" set="1"/>
      <BoundaryCondition loadtimefunction="1" dofs="1" values="0.0" set="2"/>
      <NodalLoad loadtimefunction="2" dofs="1 2" components="0.05 0.0" set="3"/>
    </BoundaryConditions>
    <InitialConditions/>
    <LoadTimeFunctions>
      <!-- TIME FUNCTION -->
      <ConstantFunction f_t_="1.0"/>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
  </Domain>
  <ExportModules>
    <errorcheck tolerance="1e-11">
      <NODE tStep="10" number="4" dof="1" unknown="d" value="4.560251e-06"/>
      <NODE tStep="10" number="8" dof="1" unknown="d" value="5.430258e-07"/>
      <NODE tStep="15" number="4" dof="1" unknown="d" value="6.893983e-06"/>
      <NODE tStep="15" number="8" dof="1" unknown="d" value="6.650745e-06"/>
      <NODE tStep="25" number="4" dof="1" unknown="d" value="1.592025e-05"/>
      <NODE tStep="25" number="8" dof="1" unknown="d" value="1.101896e-05"/>
    </errorcheck>
  </ExportModules>
</oofem>
