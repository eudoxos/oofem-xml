<oofem version="1">
  <Output>cbs2.out</Output>
  <Description>pipe test, the nondimensional version of patch02.in</Description>
  <Analysis type="cbs" nsteps="10" lstype="0" smtype="0" deltat="0.01" cmflag="0" theta1="0.5" theta2="0.5" scaleflag="1" lscale="3.0" uscale="2.0" dscale="0.3"/>
  <Domain domain="2dincompflow">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0" boundary=""/>
      <Node coords="0.0 1.0 0.0" boundary=""/>
      <Node coords="0.0 2.0 0.0" boundary=""/>
      <Node coords="2.0 0.0 0.0" boundary=""/>
      <Node id="5" coords="2.0 1.0 0.0"/>
      <Node coords="2.0 2.0 0.0" boundary=""/>
      <Node coords="4.0 0.0 0.0" boundary=""/>
      <Node coords="4.0 1.0 0.0" boundary=""/>
      <Node coords="4.0 2.0 0.0" boundary=""/>
    </Nodes>
    <Elements>
      <tr1cbs nodes="1 4 2" bsides="1 3" bcodes="6 2"/>
      <tr1cbs nodes="5 2 4" bsides=""/>
      <tr1cbs nodes="2 5 3" bsides="3" bcodes="2"/>
      <tr1cbs nodes="6 3 5" bsides=""/>
      <tr1cbs id="5" nodes="4 7 5" bsides="1" bcodes="6"/>
      <tr1cbs nodes="8 5 7" bsides="3" bcodes="8"/>
      <tr1cbs nodes="5 8 6" bsides=""/>
      <tr1cbs nodes="9 6 8" bsides="1 3" bcodes="6 8"/>
    </Elements>
    <CrossSections>
      <fluidcs mat="1" set="4"/>
    </CrossSections>
    <Materials>
      <newtonianfluid d="1.0" mu="1.0"/>
    </Materials>
    <BoundaryConditions>
      <!-- prescribed inlet velocity v = 1m/s -->
      <BoundaryCondition loadtimefunction="1" values="1.0" valtype="5" set="1" dofs="7"/>
      <!-- zero velocity (wall) condition -->
      <BoundaryCondition loadtimefunction="1" values="0.0" valtype="5" set="2" dofs="8"/>
      <!-- pressure -->
      <BoundaryCondition loadtimefunction="1" values="0.0" valtype="3" set="3" dofs="11"/>
    </BoundaryConditions>
    <InitialConditions>
      <!-- ic for velocity at inlet -->
      <InitialCondition conditions="u 1.0" valtype="5"/>
    </InitialConditions>
    <LoadTimeFunctions>
      <Piecewiselinfunction t="0.0 0.004 2.0" f_t_="0.0 1.0 1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="1 2 3"/>
      <Set nodes="1 3 4 6 7 9"/>
      <Set nodes="7 8 9"/>
      <Set elementranges="1-8"/>
    </Sets>
  </Domain>
  <ExportModules>
    <!-- Note: tests checks unscaled variables! -->
    <errorcheck>
      <NODE tStep="10" number="5" dof="7" unknown="d" value="0.485587536"/>
      <NODE tStep="10" number="8" dof="7" unknown="d" value="0.481448118"/>
      <!-- 3  6   9 -->
      <!-- 2  5   8 -->
      <!-- 1  4   7 -->
      <NODE tStep="10" number="5" dof="11" unknown="d" value="-25.3024069"/>
    </errorcheck>
  </ExportModules>
</oofem>
