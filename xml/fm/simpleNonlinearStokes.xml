      <Node coords="1.0 0.5 0.0"/>
      <Node coords="0.5 1.0 0.0"/>
    </Nodes>
    <Elements>
      <tr21stokes nodes="1 2 4 5 6 7"/>
      <tr21stokes nodes="2 3 4 8 9 6"/>
    </Elements>
    <CrossSections>
      <fluidcs mat="1" set="3"/>
    </CrossSections>
    <Materials>
      <nonlinearfluid mu="1.0" d="1.0" c="1.0" alpha="0.5"/>
    </Materials>
    <BoundaryConditions>
      <BoundaryCondition loadtimefunction="1" values="1.0 1.0 1.0" dofs="7 8 11" set="1"/>
