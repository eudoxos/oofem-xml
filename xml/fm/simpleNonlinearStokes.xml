<oofem version="1">
  <Output>simpleNonlinearStokes.out</Output>
  <Description>Test for stokes flow on triangular Taylor-Hood element</Description>
  <Analysis type="stokesflow" nsteps="1" rtolf="1e-05" lstype="0" smtype="0" linesearch="1" maxiter="50"/>
  <Domain domain="2dincompflow">
    <OutputManager tstep_all="" dofman_all="" element_all=""/>
    <Nodes>
      <Node coords="0.0 0.0 0.0"/>
      <Node coords="1.0 0.0 0.0"/>
      <Node coords="1.0 1.0 0.0"/>
      <Node coords="0.0 1.0 0.0"/>
      <Node id="5" coords="0.5 0.0 0.0"/>
