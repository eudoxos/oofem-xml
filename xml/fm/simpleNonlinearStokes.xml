    <InitialConditions/>
    <LoadTimeFunctions>
      <ConstantFunction f_t_="1.0"/>
    </LoadTimeFunctions>
    <Sets>
      <Set nodes="2 3"/>
      <Set nodes="1 4"/>
      <Set elementranges="1-2"/>
    </Sets>
  </Domain>
  <ExportModules>
    <errorcheck>
      <NODE tStep="1" number="4" dof="8" unknown="d" value="0.6939245"/>
      <NODE tStep="1" number="4" dof="11" unknown="d" value="-2.845086"/>
    </errorcheck>
  </ExportModules>
</oofem>
